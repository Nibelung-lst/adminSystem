AAKA,QAAA,UAAA,EAAA,WALA;AAMA,QAAA,UAAA,EAAA;AANA,OADA,CApCA;AA8CA;AACA,MAAA,QAAA,EAAA,CA/CA;AAgDA;AACA,MAAA,SAAA,EAAA,CAjDA;AAkDA;AACA,MAAA,WAAA,EAAA;AAnDA,KAAA;AAqDA,GAxDA;AAyDA,EAAA,OAzDA,qBAyDA;AACA,SAAA,iBAAA;AACA,GA3DA;AA4DA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,gBAFA,4BAEA,GAFA,EAEA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,KAJA;AAKA;AACA,IAAA,mBANA,+BAMA,GANA,EAMA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,KARA;AASA;AACA,IAAA,YAVA,0BAUA;AAAA;;AACA,UAAA,OAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA;AADA,OAAA;AAGA,MAAA,UAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,IAAA,GAAA,CAAA,IAAA,IAAA,IAAA,EAAA,CAEA,CAFA,MAEA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OANA;AAOA,KArBA;AAsBA;AACA,IAAA,SAvBA,uBAuBA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAzBA;AA0BA,IAAA,cA1BA,4BA0BA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,KA5BA;AA6BA,IAAA,gBA7BA,8BA6BA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,UAAA,UAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,YAAA,CAAA,IADA;AAEA,QAAA,KAAA,EAAA,KAAA,YAAA,CAAA,KAFA;AAGA,QAAA,UAAA,EAAA,KAAA,YAAA,CAAA;AAHA,OAAA;AAKA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,KArCA;AAsCA,IAAA,kBAtCA,8BAsCA,GAtCA,EAsCA,MAtCA,EAsCA;AAAA;;AACA,UAAA,kBAAA,GAAA;AACA,QAAA,OAAA,EAAA,GAAA,CAAA,EADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,MAAA,gBAAA,CAAA,kBAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,OANA;AAOA,KAlDA;AAmDA,IAAA,yBAnDA,qCAmDA,GAnDA,EAmDA;AACA,WAAA,UAAA,CAAA,MAAA,CAAA,UAAA,MAAA,EAAA;AACA,YAAA,GAAA,CAAA,OAAA,CAAA,MAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,iBAAA,IAAA;AACA;AACA,OAJA;AAKA,KAzDA;AA0DA;AACA,IAAA,iBA3DA,+BA2DA;AAAA;;AACA,MAAA,SAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,MAAA,CAAA,SAAA,CAAA,MAAA;AACA,OAHA;AAIA;AAhEA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <main>\r\n    <el-row :gutter=\"12\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :span=\"4\">\r\n              <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item>管理员列表</el-breadcrumb-item>\r\n              </el-breadcrumb>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--搜索栏-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :span=\"5\">\r\n              <el-input\r\n                clearable\r\n                placeholder=\"请输入管理员名称\"\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"name\">\r\n              </el-input>\r\n            </el-col>\r\n            <el-col :offset='15' :span=\"1\">\r\n              <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n            </el-col>\r\n            <el-col :offset='1' :span=\"1\">\r\n              <el-button @click=\"handleAdd\" type=\"primary\">添加</el-button>\r\n            </el-col>\r\n            <el-dialog\r\n              title=\"添加管理员\"\r\n              :visible.sync=\"dialogAddAdmin\"\r\n              width=\"30%\">\r\n              <el-form :model=\"formAddAdmin\">\r\n                <el-form-item :label-width=\"'100px'\" label=\"名称\">\r\n                  <el-input autocomplete=\"off\" v-model=\"formAddAdmin.name\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item :label-width=\"'100px'\" label=\"手机号\">\r\n                  <el-input autocomplete=\"off\" v-model=\"formAddAdmin.phone\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item :label-width=\"'100px'\" label=\"角色\">\r\n                <el-checkbox-group v-model=\"formAddAdmin.systemList\"\r\n                                   @change=\"handelChangeCheckBoxGroup\">\r\n                  <el-row>\r\n                    <el-col :span=\"12\" :key=\"system.code\" v-for=\"system in systemList\"\r\n                            style=\"margin-bottom: 15px\">\r\n                      <el-checkbox\r\n                        size=\"medium\" border\r\n                        :label=\"system.name\"\r\n                        :name=\"system.code\"></el-checkbox>\r\n                    </el-col>\r\n                  </el-row>\r\n                </el-checkbox-group>\r\n                </el-form-item>\r\n              </el-form>\r\n              <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n            </el-dialog>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--数据表格-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-table\r\n          :data=\"tableData\"\r\n          border\r\n          style=\"width: 100%\"\r\n          tooltip-effect=\"dark\">\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"名称\">\r\n            <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"手机号\">\r\n            <template slot-scope=\"scope\">{{ scope.row.phone }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"创建时间\">\r\n            <template slot-scope=\"scope\">{{ scope.row.createTime }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"更新时间\">\r\n            <template slot-scope=\"scope\">{{ scope.row.updateTime }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"当前状态\"\r\n            prop=\"status\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button :type=\"scope.row.status===0?'primary':'danger'\"\r\n                         plain size=\"small\"\r\n                         @click=\"handleUpdateStatus(scope.row,scope.row.status=scope.row.status===1?0:1)\">\r\n                {{ scope.row.status === 0 ? '正常' : '已禁用' }}\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            prop=\"id\"\r\n            label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"primary\" plain size=\"small\">编辑</el-button>\r\n              <el-button type=\"danger\" plain size=\"small\" @click=\"handleUpdateStatus(scope.row,-1)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"角色\">\r\n            <template slot-scope=\"\">\r\n              <el-button type=\"primary\" plain size=\"small\">查看详情</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n    <!--分页-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :offset=\"10\" :span=\"12\">\r\n              <div class=\"block\">\r\n                <el-pagination\r\n                  :current-page=currentPage\r\n                  :page-size=pageSize\r\n                  :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                  :total=totalPage\r\n                  @current-change=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { amendAdminStatus, findByName, listAdmin } from '../api/admin'\r\n\r\nexport default {\r\n  name: 'adminList',\r\n  data () {\r\n    return {\r\n      name: '',\r\n      dialogAddAdmin: false,\r\n      formAddAdmin: {\r\n        name: '',\r\n        phone: 0,\r\n        systemList: []\r\n      },\r\n      systemList: [\r\n        {\r\n          id: 7,\r\n          code: '0',\r\n          name: '角色A'\r\n        },\r\n        {\r\n          id: 1,\r\n          code: '1',\r\n          name: '角色BBBBBB'\r\n        },\r\n        {\r\n          id: 2,\r\n          code: '2',\r\n          name: '角色C'\r\n        },\r\n        {\r\n          id: 3,\r\n          code: '3',\r\n          name: '角色D'\r\n        },\r\n        {\r\n          id: 4,\r\n          code: '4',\r\n          name: '角色E'\r\n        }\r\n      ],\r\n      // 数据表格\r\n      tableData: [\r\n        {\r\n          id: '1',\r\n          name: 'zhangsan',\r\n          phone: '17767672525',\r\n          status: 1,\r\n          createTime: '2019-9-10',\r\n          updateTime: '2019-10-10'\r\n        }\r\n      ],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  created () {\r\n    this.handleGetAllAdmin()\r\n  },\r\n  methods: {\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n    },\r\n    // 根据用户名查找用户\r\n    handleSearch () {\r\n      let adminAO = {\r\n        name: this.name\r\n      }\r\n      findByName(adminAO).then(res => {\r\n        if (res.code === 0 && res.data != null) {\r\n\r\n        } else {\r\n          this.$message.error('查询为空')\r\n        }\r\n      })\r\n    },\r\n    // 添加用户\r\n    handleAdd () {\r\n      this.dialogAddAdmin = true\r\n    },\r\n    handleAddClose () {\r\n      this.dialogAddAdmin = false\r\n    },\r\n    handleAddSuccess () {\r\n      this.dialogAddAdmin = false\r\n      let addAdminAO = {\r\n        name: this.formAddAdmin.name,\r\n        phone: this.formAddAdmin.phone,\r\n        systemList: this.formAddAdmin.systemList\r\n      }\r\n      console.log(addAdminAO)\r\n    },\r\n    handleUpdateStatus (row, status) {\r\n      let amendAdminStatusAO = {\r\n        adminId: row.id,\r\n        status: status\r\n      }\r\n      amendAdminStatus(amendAdminStatusAO).then(res => {\r\n        if (res.code === 0) {\r\n          this.$message.success('修改成功!')\r\n        } else {\r\n          this.$message.error(res.message)\r\n        }\r\n      })\r\n    },\r\n    handelChangeCheckBoxGroup (val) {\r\n      this.systemList.filter(system => {\r\n        if (val.indexOf(system.name) >= 0) {\r\n          return true\r\n        }\r\n      })\r\n    },\r\n    // TODO 获取所有管理员\r\n    handleGetAllAdmin () {\r\n      listAdmin().then(res => {\r\n        this.tableData = res.data\r\n        this.totalPage = this.tableData.length\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}