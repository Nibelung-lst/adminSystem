A,4BAmHA;AAAA;;AACA,WAAA,QAAA,CAAA,SAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,OAPA,EAQA,KARA,CAQA,YAAA,CAAA,CARA;AASA,KA7HA;AA8HA,IAAA,gBA9HA,8BA8HA;AAAA;;AACA,UAAA,YAAA,GAAA;AACA,mBAAA,KAAA,UAAA,CAAA,OADA;AAEA,mBAAA,KAAA,UAAA,CAAA,OAFA;AAGA,uBAAA,KAAA,UAAA,CAAA,WAHA;AAIA,kBAAA,KAAA,UAAA,CAAA,MAJA;AAKA,wBAAA,KAAA,YALA;AAMA,qBAAA,KAAA,UAAA,CAAA;AANA,OAAA;AAQA,WAAA,MAAA,CAAA,IAAA,CAAA,mDAAA,EAAA,YAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AACA,UAAA,OAAA,CAAA,IAAA;;AACA,UAAA,OAAA,CAAA,iBAAA,GAAA,KAAA;AACA,SAJA,MAIA;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA,OARA;AASA;AAhJA;AAhDA,CAAA","sourcesContent":["<template>\r\n<main>\r\n  <!--导航栏-->\r\n  <el-row :gutter=\"12\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :span=\"4\">\r\n            <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n              <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n              <el-breadcrumb-item>商品spu管理</el-breadcrumb-item>\r\n            </el-breadcrumb>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--搜索栏-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :span=\"5\">\r\n            <el-input\r\n              clearable\r\n              placeholder=\"请输入分类名\"\r\n              prefix-icon=\"el-icon-search\"\r\n              v-model=\"name\">\r\n            </el-input>\r\n          </el-col>\r\n          <el-col :offset='15' :span=\"1\">\r\n            <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n          </el-col>\r\n          <el-col :offset='1' :span=\"1\">\r\n            <el-button @click=\"handleAdd\" type=\"primary\">添加</el-button>\r\n          </el-col>\r\n          <!--添加分类-->\r\n          <el-dialog\r\n            title=\"添加商品\"\r\n            :visible.sync=\"dialogAddCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddSpu\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"spu商品编号\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddSpu.spuCode\" @blur=\"check\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"商品名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddSpu.spuName\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"商品描述\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddSpu.spuDescribe\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"商品品牌\">\r\n                <el-select v-model=\"formAddSpu.brandCode\" filterable placeholder=\"请选择\">\r\n                  <el-option\r\n                    autocomplete=\"off\"\r\n                    v-for=\"item in options\"\r\n                    :key=\"item.code\"\r\n                    :label=\"item.name\"\r\n                    :value=\"item.code\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formAddSpu.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--修改Spu-->\r\n          <el-dialog\r\n            title=\"修改spu\"\r\n            :visible.sync=\"dialogUpdateCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formUpdateSpu\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateSpu.spuCode\" type=\"hidden\"></el-input>\r\n              <el-form-item :label-width=\"'100px'\" label=\"商品名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateSpu.spuName\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"商品描述\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateSpu.spuDescribe\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formUpdateSpu.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleUpdateClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleUpdateSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--数据表格-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\"\r\n        tooltip-effect=\"dark\">\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n              <el-form-item label=\"商品描述\">\r\n                <span>{{ props.row.spuDescribe }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"创建人\">\r\n                <span>{{ props.row.creatorName }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"创建时间\">\r\n                <span>{{ props.row.creatorTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"最后修改人\">\r\n                <span>{{ props.row.LastModifyName }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"最后修改人\">\r\n                <span>{{ props.row.LastModifyTime }}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"商品编号\">\r\n          <template slot-scope=\"scope\">{{ scope.row.spuCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"品牌名\">\r\n          <template slot-scope=\"scope\">{{ scope.row.brandName }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"商品名\">\r\n          <template slot-scope=\"scope\">{{ scope.row.spuName }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"当前状态\"\r\n          prop=\"status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button :type=\"scope.row.status===true?'primary':'danger'\"\r\n                       plain size=\"small\"\r\n                       @click=\"handleUpdateStatus(scope.row)\">\r\n              {{ scope.row.status === true ? '正常' : '不启用' }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleUpdate(scope.row)\">编辑</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"其他\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleSearchSku(scope.row)\">查看SKU</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-col>\r\n  </el-row>\r\n  <!--分页-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :offset=\"10\" :span=\"12\">\r\n            <div class=\"block\">\r\n              <el-pagination\r\n                :current-page=currentPage\r\n                :page-size=pageSize\r\n                :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                :total=totalPage\r\n                @current-change=\"handleCurrentChange\"\r\n                @size-change=\"handleSizeChange\"\r\n                background\r\n                layout=\"total, sizes, prev, pager, next, jumper\">\r\n              </el-pagination>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n</main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  mounted () {\r\n    this.send()\r\n  },\r\n  name: 'ProductCategory',\r\n  data () {\r\n    return {\r\n      categoryCode: this.$route.query.categoryCode,\r\n      name: '',\r\n      dialogUpdateCategory: false,\r\n      dialogAddCategory: false,\r\n      formUpdateSpu: {\r\n        spuCode: '',\r\n        spuName: '',\r\n        spuDescribe: '',\r\n        status: ''\r\n      },\r\n      formAddSpu: {\r\n        spuCode: '',\r\n        spuName: '',\r\n        spuDescribe: '',\r\n        brandCode: '',\r\n        status: true\r\n      },\r\n      parentCategoryList: [\r\n        {\r\n          code: 0,\r\n          name: '分类A'\r\n        },\r\n        {\r\n          code: 1,\r\n          name: '分类B'\r\n        }\r\n      ],\r\n      options: [],\r\n      // 数据表格\r\n      tableData: [],\r\n      loading: true,\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1,\r\n      allCategoryUrl: 'http://localhost:8080/adminback/api/getSpuByCategoryCode' + '?categoryCode=',\r\n      searchCategoryUrl: 'http://localhost:8080/adminback/api/searchCategory' + '?name='\r\n    }\r\n  },\r\n  methods: {\r\n    handleSearchSku (row) {\r\n      this.$router.push({ path: '/product/Sku', query: { spuCode: row.spuCode } })\r\n    },\r\n    check () {\r\n      let check = this.formAddSpu.spuCode\r\n      this.$axios.post('http://localhost:8080/adminback/api/checkSpuCode' + '?spuCode=' + check).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$message({\r\n            message: '编号重复',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '编号可使用',\r\n            type: 'success'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    searchChildren (row) {\r\n      this.$router.push({ path: '/product/CategoryChildren', query: { categoryCode: row.categoryCode } })\r\n    },\r\n    send () {\r\n      this.$axios.post(this.allCategoryUrl + this.categoryCode).then(res => {\r\n        this.tableData = res.data.data.list\r\n        this.totalPage = res.data.data.total\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n      this.$axios.post(this.allCategoryUrl + this.categoryCode + '&pageSize=' + val + '&currentPage=' + this.currentPage, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.$axios.post(this.allCategoryUrl + this.categoryCode + '&pageNumber=' + val + '&pageSize=' + this.pageSize, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    // 修改分类名\r\n    handleUpdateSuccess () {\r\n      let productSpuAO = {\r\n        'spuCode': this.formUpdateSpu.spuCode,\r\n        'spuName': this.formUpdateSpu.spuName,\r\n        'spuDescribe': this.formUpdateSpu.spuDescribe,\r\n        'status': this.formUpdateSpu.status\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/updateSpu', productSpuAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('修改成功')\r\n          this.send()\r\n          this.dialogUpdateCategory = false\r\n        } else {\r\n          this.$message.error('未知错误')\r\n        }\r\n      })\r\n    },\r\n    handleSearch () {\r\n      this.$axios.post(this.searchCategoryUrl).then(res => {\r\n        if (res.data.code === 10000) {\r\n          this.tableData = res.data.data.list\r\n          this.allCategoryUrl = this.searchCategoryUrl\r\n        } else {\r\n          this.$message.error('未知错误')\r\n        }\r\n      })\r\n    },\r\n    handleUpdateClose () {\r\n      this.dialogUpdateCategory = false\r\n    },\r\n    // 修改商品\r\n    handleUpdate (row) {\r\n      this.formUpdateSpu.spuCode = row.spuCode\r\n      this.formUpdateSpu.spuDescribe = row.spuDescribe\r\n      this.formUpdateSpu.spuName = row.spuName\r\n      this.formUpdateSpu.status = row.status\r\n      this.dialogUpdateCategory = true\r\n    },\r\n    handleUpdateStatus (row) {\r\n      this.$confirm('此操作将修改商品状态，是否继续?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        row.status = row.status === false\r\n        let productSpuAO = {\r\n          'spuCode': row.spuCode,\r\n          'status': row.status\r\n        }\r\n        this.$axios.post('http://localhost:8080/adminback/api/updateSpuStatue', productSpuAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('修改成功')\r\n          }\r\n        })\r\n      })\r\n    },\r\n    // 添加spu\r\n    handleAdd () {\r\n      this.$axios.post('http://localhost:8080/adminback/api/getBrandByCategoryCode' + '?categoryCode=' + this.categoryCode).then(res => {\r\n        this.options = res.data.data\r\n      })\r\n      this.dialogAddCategory = true\r\n    },\r\n    handleAddClose () {\r\n      this.$confirm('是否确认取消?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message.success('open')\r\n        this.dialogAddCategory = false\r\n      })\r\n        .catch(() => {})\r\n    },\r\n    handleAddSuccess () {\r\n      let productSpuAO = {\r\n        'spuCode': this.formAddSpu.spuCode,\r\n        'spuName': this.formAddSpu.spuName,\r\n        'spuDescribe': this.formAddSpu.spuDescribe,\r\n        'status': this.formAddSpu.status,\r\n        'categoryCode': this.categoryCode,\r\n        'brandCode': this.formAddSpu.brandCode\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/addProductSpu', productSpuAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加商品成功!')\r\n          this.send()\r\n          this.dialogAddCategory = false\r\n        } else {\r\n          this.$message.error('都跟你讲编号错误了!')\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/product"}]}