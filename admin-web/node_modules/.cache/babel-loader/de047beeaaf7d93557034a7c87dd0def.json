el=\"最低消费\">\r\n            <template slot-scope=\"scope\">\r\n              {{ scope.row.minConsumption }}\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"操作\">\r\n            <template slot-scope=\"\">\r\n              <el-button type=\"primary\" plain size=\"small\">编辑</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n    <!--分页-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :offset=\"10\" :span=\"12\">\r\n              <div class=\"block\">\r\n                <el-pagination\r\n                  :current-page=currentPage\r\n                  :page-size=pageSize\r\n                  :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                  :total=totalPage\r\n                  @current-change=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { userLevelList } from '../../api/user'\r\n\r\nexport default {\r\n  created () {\r\n    this.handleLevelList()\r\n  },\r\n  mounted () {\r\n\r\n  },\r\n  data () {\r\n    return {\r\n      // 搜索栏\r\n      searchData: {\r\n        name: '',\r\n        levelId: ''\r\n      },\r\n      name: '',\r\n      levelId: [\r\n        {\r\n          value: 1,\r\n          label: '青铜'\r\n        },\r\n        {\r\n          value: 2,\r\n          label: '白银'\r\n        },\r\n        {\r\n          value: 3,\r\n          label: '黄金'\r\n        },\r\n        {\r\n          value: 4,\r\n          label: '钻石'\r\n        },\r\n        {\r\n          value: 5,\r\n          label: '星耀'\r\n        }\r\n      ],\r\n      // 数据\r\n      tableData: [\r\n        {\r\n          id: '1',\r\n          levelName: '白银',\r\n          levelDiscount: '0.5',\r\n          minConsumption: 100\r\n        }\r\n      ],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  methods: {\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.handleLevelList()\r\n    },\r\n    // 查询\r\n    handleSearch () {\r\n    },\r\n    // 初始化数据\r\n    handleLevelList () {\r\n      let params = {\r\n        'pageNum': this.currentPage,\r\n        'pageSize': this.pageSize\r\n      }\r\n      userLevelList(params).then(res => {\r\n        this.totalPage = res.data.count\r\n        this.tableData = res.data.content\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n\r\n  .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n\r\n  .el-upload--picture-card {\r\n    background-color: #fbfdff;\r\n    border: 1px dashed #c0ccda;\r\n    border-radius: 6px;\r\n    -webkit-box-sizing: border-box;\r\n    box-sizing: border-box;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 176px;\r\n    vertical-align: top;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/user"}]}