AtBA,EAsBA;AACA,WAAA,OAAA,GAAA,GAAA;AACA,WAAA,WAAA,CAAA,OAAA,GAAA,GAAA;AACA,WAAA,OAAA;AACA;AA1BA;AA1GA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n\r\n  <!--  <el-row :gutter=\"12\" shadow=\"never\" style=\"background-color: #ffffff\">-->\r\n\r\n    <el-card class=\"filter-container\" shadow=\"never\">\r\n        <el-form :inline=\"true\" :model=\"status\" size=\"small\" label-width=\"140px\">\r\n\r\n          <el-form-item>\r\n            <template >\r\n              <el-select v-model=\"value\" placeholder=\"请选择\" >\r\n                <el-option\r\n                  v-for=\"item in options\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </template>\r\n          </el-form-item>\r\n\r\n          <el-form-item>\r\n            <el-input v-model=\"input\" placeholder=\"请输入内容\" ></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button\r\n              type=\"primary\"\r\n              @click=\"handleSearchList()\"\r\n              size=\"small\">\r\n              查询\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n    </el-card>\r\n    <el-card class=\"table-container\" style=\"margin-top: 10px;text-align: center\" shadow=\"never\" >\r\n      <el-table\r\n                :data=\"list\"\r\n                style=\"width: 100%\"\r\n                >\r\n        <el-table-column label=\"员工编号\" align=\"center\" width=\"200\">\r\n          <template slot-scope=\"scope\">{{scope.row.orderCode===null?'-':scope.row.orderCode}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"员工姓名\" width=\"140\" align=\"center\">\r\n          <template slot-scope=\"scope\">{{scope.row.userName}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"员工职位\" width=\"100\" align=\"center\">\r\n          <template slot-scope=\"scope\">{{scope.row.type | formatType}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"联系方式\" width=\"160\" align=\"center\">\r\n          <template slot-scope=\"scope\">{{scope.row.createTime | formatDate}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"在岗状态\" width=\"160\" align=\"center\">\r\n          <template slot-scope=\"scope\">{{scope.row.useTime | formatDate}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"最近一天上班时间\" width=\"140\" align=\"center\">\r\n          <template slot-scope=\"scope\">{{scope.row.status | formatCouponStatus}}</template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n    <div class=\"pagination-container\">\r\n          <el-card shadow=\"never\">\r\n            <el-row :gutter=\"12\">\r\n              <el-col :offset=\"10\" :span=\"12\">\r\n                <div class=\"block\">\r\n                  <el-pagination\r\n                    :current-page=pageNum\r\n                    :page-size=pageSize\r\n                    :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                    :total=total\r\n                    @current-change=\"handleCurrentChange\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    background\r\n                    layout=\"total, sizes, prev, pager, next, jumper\">\r\n                  </el-pagination>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { formatDate } from '@/utils/date'\r\nimport { getCouponByCode, getUserCouponList } from '@/api/marketing'\r\n\r\nexport default {\r\n  name: 'CouponHistory',\r\n  data () {\r\n    return {\r\n      options: [{\r\n        value: '选项1',\r\n        label: '员工编号'\r\n      }, {\r\n        value: '选项2',\r\n        label: '员工姓名'\r\n      }, {\r\n        value: '选项3',\r\n        label: '员工职位'\r\n      }],\r\n      value: '',\r\n\r\n\r\n      couponStatusOptions: [\r\n        {\r\n          label: '未使用',\r\n          value: 1\r\n        },\r\n        {\r\n          label: '已使用',\r\n          value: 2\r\n        }\r\n      ],\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 5\r\n      },\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      status: 1,\r\n      total: 0,\r\n      list: null,\r\n      coupon: {\r\n      },\r\n      couponCode: 1\r\n    }\r\n  },\r\n  mounted () {\r\n    let params = 'couponCode=' + this.$route.query.couponCode\r\n    getCouponByCode(params).then(res => {\r\n        this.coupon = res.data;\r\n      console.log(res.data)\r\n      console.log('coupon' )\r\n      console.log(this.coupon)\r\n    })\r\n    this.couponCode = this.$route.query.couponCode\r\n    this.getData()\r\n    // console.log(this, list)\r\n  },\r\n  created () {\r\n\r\n  },\r\n  filters: {\r\n    formatType (type) {\r\n      if (type === 1) {\r\n        return '用户领取'\r\n      } else{\r\n        return '后台赠送'\r\n      }\r\n    },\r\n    formatUseType (useType) {\r\n      if (useType === 1) {\r\n        return '全场通用'\r\n      } else if (useType === 2) {\r\n        return '指定分类'\r\n      } else {\r\n        return '指定商品'\r\n      }\r\n    },\r\n    formatDate (time) {\r\n      if (time == null || time === '') {\r\n        return '-'\r\n      }\r\n      let date = new Date(time)\r\n      return formatDate(date, 'yyyy-MM-dd mm:ss')\r\n    },\r\n    formatStatus (status) {\r\n      if (status === 1) {\r\n        return '已上线'\r\n      } else {\r\n        return '下线'\r\n      }\r\n    },\r\n    formatCouponStatus (status) {\r\n      if (status == 1) {\r\n        return '未使用'\r\n      } else if (status == 2) {\r\n        return '已使用'\r\n      } else {\r\n        return '已过期'\r\n      }\r\n    },\r\n    formatExpire (endTime) {\r\n      let now = new Date().getTime()\r\n      let endDate = new Date(endTime)\r\n      if (endDate > now) {\r\n        return '未过期'\r\n      } else {\r\n        return '已过期'\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    getData () {\r\n      this.listLoading = true\r\n      let params = this.queryParams\r\n      getUserCouponList(params).then(res => {\r\n        if (res.code === 0) {\r\n          this.listLoading = false\r\n          this.list = res.data.content\r\n          this.total = res.data.count\r\n        }\r\n      })\r\n    },\r\n\r\n    handleSearchList () {\r\n      this.pageNum = 1\r\n      this.getData()\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.pageNum = val\r\n        this.queryParams.pageNum = val\r\n      this.getData();\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n  .app-container {\r\n    width: 80%;\r\n    margin: 10px auto;\r\n  }\r\n  .filter-container {\r\n    margin-top: 20px;\r\n  }\r\n  .table-layout {\r\n    margin-top: 20px;\r\n    border-left: 1px solid #DCDFE6;\r\n    border-top: 1px solid #DCDFE6;\r\n  }\r\n  .table-cell {\r\n    height: 60px;\r\n    line-height: 40px;\r\n    border-left: 1px solid #DCDFE6;\r\n    border-right: 1px solid #DCDFE6;\r\n    border-bottom: 1px solid #DCDFE6;\r\n    padding: 10px;\r\n    font-size: 14px;\r\n    color: #606266;\r\n    text-align: center;\r\n    overflow: hidden;\r\n  }\r\n  .table-cell-title {\r\n    border-left: 1px solid #DCDFE6;\r\n    border-right: 1px solid #DCDFE6;\r\n    border-bottom: 1px solid #DCDFE6;\r\n    padding: 10px;\r\n    background: #F2F6FC;\r\n    text-align: center;\r\n    font-size: 14px;\r\n    color: #303133;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/marketing"}]}