late>\r\n<main>\r\n  <!--导航栏-->\r\n  <el-row :gutter=\"12\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :span=\"4\">\r\n            <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n              <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n              <el-breadcrumb-item>SKU管理</el-breadcrumb-item>\r\n            </el-breadcrumb>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--搜索栏-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :span=\"5\">\r\n            <el-input\r\n              clearable\r\n              placeholder=\"请输入分类名\"\r\n              prefix-icon=\"el-icon-search\"\r\n              v-model=\"name\">\r\n            </el-input>\r\n          </el-col>\r\n          <el-col :offset='11' :span=\"2\">\r\n            <el-button @click=\"openSpecs\" type=\"primary\" style=\"margin-left: 16px;\">\r\n              点我打开\r\n            </el-button>\r\n          </el-col>\r\n          <el-col :offset='1' :span=\"1\">\r\n            <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n          </el-col>\r\n          <el-col :offset='1' :span=\"1\">\r\n            <el-button @click=\"handleAdd\" type=\"primary\">添加</el-button>\r\n          </el-col>\r\n          <!--抽屉-------------------------------------------------------------------------->\r\n          <el-drawer\r\n            title=\"属性\"\r\n            :visible.sync=\"drawer\"\r\n            size=\"50%\">\r\n            <el-button style=\"margin-left: 640px; margin-bottom: 20px\" type=\"primary\" plain size=\"small\" @click=\"addSpecskey\">添加属性</el-button>\r\n            <el-table :data=\"gridData\"\r\n                      border\r\n                      style=\"width: 100%\"\r\n                      tooltip-effect=\"dark\">\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"属性名\">\r\n                <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"创建人\">\r\n                <template slot-scope=\"scope\">{{ scope.row.creatorName }}</template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"创建时间\">\r\n                <template slot-scope=\"scope\">{{ scope.row.creatorTime }}</template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"操作\">\r\n                <template slot-scope=\"scope\">\r\n                  <el-button type=\"primary\" plain size=\"small\" @click=\"SpaceValue(scope.row)\">查看</el-button>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n            <div>\r\n              <!--------第二个表格----------->\r\n              <el-drawer\r\n                title= \"属性值\"\r\n                :append-to-body=\"true\"\r\n                :before-close=\"handleClose\"\r\n                :visible.sync=\"innerDrawer\">\r\n                <el-button style=\"margin-left: 375px; margin-bottom: 20px\" type=\"primary\" plain size=\"small\" @click=\"addSpecsValue\">添加值</el-button>\r\n                <el-table :data=\"innerData\"\r\n                          border\r\n                          style=\"width: 100%\"\r\n                          tooltip-effect=\"dark\">\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"属性值\">\r\n                    <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"创建人\">\r\n                    <template slot-scope=\"scope\">{{ scope.row.creatorName }}</template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"创建时间\">\r\n                    <template slot-scope=\"scope\">{{ scope.row.creatorTime }}</template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"操作\">\r\n                    <template slot-scope=\"\">\r\n                      <el-button type=\"primary\" plain size=\"small\" >编辑</el-button>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </el-drawer>\r\n            </div>\r\n          </el-drawer>\r\n          <!--添加规格-->\r\n          <el-dialog\r\n            title=\"为SKU添加规格\"\r\n            :visible.sync=\"dialogAddSpecs\"\r\n            width=\"30%\">\r\n            <template v-for='(col) in data1'>\r\n              <el-select clearable\r\n                         size=\"small\"\r\n                         style=\"width: 135px;\"\r\n                         v-model=\"col.name\"\r\n                         v-bind:key=\"col\">\r\n                <el-option v-for=\"row in col.value\"\r\n                           :key=\"row.id\"\r\n                           :label=\"row.name\"\r\n                           :value=\"row.id\"></el-option>\r\n              </el-select>\r\n            </template>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddSpecsClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSpecsSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--------------------------------添加-------------------------------------------->\r\n          <el-dialog\r\n            title=\"添加属性\"\r\n            :visible.sync=\"dialogAddSpecsValue\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddSpecsValue\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"属性值\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddSpecsValue.name\"></el-input>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddValueClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddValueSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--------------------------------添加-------------------------------------------->\r\n          <el-dialog\r\n            title=\"添加属性\"\r\n            :visible.sync=\"dialogAddSpecsKey\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddSpecsKey\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"属性名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddSpecsKey.name\"></el-input>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddKeyClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddKeySuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--添加分类---------------------------------------------------------------------->\r\n          <el-dialog\r\n            title=\"添加SKU\"\r\n            :visible.sync=\"dialogAddCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddCategory\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku名字\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.name\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku编号\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.skuCode\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku描述\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.describe\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku原价\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.originPrice\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku现价\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.currentPrice\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-button type=\"primary\" @click=\"handleAddSku\">添加规格</el-button>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formAddCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--修改分类-->\r\n          <el-dialog\r\n            title=\"修改分类\"\r\n            :visible.sync=\"dialogUpdateCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formUpdateCategory\">\r\n              <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.categoryCode\" type=\"hidden\"></el-input>\r\n              <el-form-item :label-width=\"'100px'\" label=\"分类名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.name\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formUpdateCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleUpdateClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleUpdateSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--数据表格-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\"\r\n        tooltip-effect=\"dark\">\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n              <el-form-item label=\"创建人\">\r\n                <span>{{ props.row.creatorName }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"创建时间\">\r\n                <span>{{ props.row.creatorTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"最后修改人\">\r\n                <span>{{ props.row.LastModifyName }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"最后修改人\">\r\n                <span>{{ props.row.LastModifyTime }}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"唯一编码\">\r\n          <template slot-scope=\"scope\">{{ scope.row.uniqueCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU编号\">\r\n          <template slot-scope=\"scope\">{{ scope.row.skuCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU名\">\r\n          <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU描述\">\r\n          <template slot-scope=\"scope\">{{ scope.row.describe }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU初始价格\">\r\n          <template slot-scope=\"scope\">{{ scope.row.originPrice }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU现价\">\r\n          <template slot-scope=\"scope\">{{ scope.row.currentPrice }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"当前状态\"\r\n          prop=\"status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button :type=\"scope.row.status===true?'primary':'danger'\"\r\n                       plain size=\"small\"\r\n                       @click=\"handleUpdateStatus(scope.row)\">\r\n              {{ scope.row.status === true ? '正常' : '不启用' }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleUpdate(scope.row)\">编辑</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"其他\">\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n    </el-col>\r\n  </el-row>\r\n  <!--分页-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :offset=\"10\" :span=\"12\">\r\n            <div class=\"block\">\r\n              <el-pagination\r\n                :current-page=currentPage\r\n                :page-size=pageSize\r\n                :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                :total=totalPage\r\n                @current-change=\"handleCurrentChange\"\r\n                @size-change=\"handleSizeChange\"\r\n                background\r\n                layout=\"total, sizes, prev, pager, next, jumper\">\r\n              </el-pagination>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n</main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  mounted () {\r\n    this.send()\r\n  },\r\n  name: 'ProductCategory',\r\n  data () {\r\n    return {\r\n      attributeKeyId: '',\r\n      dialogAddSpecsValue: false,\r\n      formAddSpecsValue: {\r\n        name: ''\r\n      },\r\n      formAddSpecsKey: {\r\n        name: ''\r\n      },\r\n      dialogAddSpecsKey: false,\r\n      innerData: [],\r\n      gridData: [],\r\n      uniqueCodeAO: [],\r\n      data1: [],\r\n      spuCode: this.$route.query.spuCode,\r\n      name: '',\r\n      drawer: false,\r\n      innerDrawer: false,\r\n      dialogAddSpecs: false,\r\n      dialogUpdateCategory: false,\r\n      dialogAddCategory: false,\r\n      dialogAddBrand: false,\r\n      formUpdateCategory: {\r\n        categoryCode: '',\r\n        name: '',\r\n        status: true\r\n      },\r\n      formAddCategory: {\r\n        name: '',\r\n        originPrice: '',\r\n        currentPrice: '',\r\n        skuCode: '',\r\n        describe: '',\r\n        status: true\r\n      },\r\n      // 数据表格\r\n      tableData: [],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1,\r\n      allCategoryUrl: 'http://localhost:8080/adminback/api/getSkuBySpuCode' + '?spuCode='\r\n    }\r\n  },\r\n  methods: {\r\n    handleAddValueSuccess () {\r\n      let specsValueAO = {\r\n        'name': this.formAddSpecsValue.name,\r\n        'attributeKeyId': this.attributeKeyId\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/addSpecsValue', specsValueAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加成功!')\r\n          this.dialogAddSpecsValue = false\r\n        } else {\r\n          this.$message.error('添加失败!')\r\n        }\r\n      })\r\n    },\r\n    addSpecsValue () {\r\n      this.dialogAddSpecsValue = true\r\n    },\r\n    handleAddValueClose () {\r\n      this.dialogAddSpecsValue = false\r\n    },\r\n    handleAddKeySuccess () {\r\n      let specsKeyAO = {\r\n        'name': this.formAddSpecsKey.name,\r\n        'spuCode': this.spuCode\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/addSpecsKey', specsKeyAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加成功!')\r\n          this.dialogAddSpecsKey = false\r\n          this.openSpecs()\r\n        } else {\r\n          this.$message.error('添加失败!')\r\n        }\r\n      })\r\n    },\r\n    handleAddKeyClose () {\r\n      this.dialogAddSpecsKey = false\r\n    },\r\n    addSpecskey () {\r\n      this.dialogAddSpecsKey = true\r\n    },\r\n    SpaceValue (row) {\r\n      this.attributeKeyId = row.id\r\n      this.$axios.post('http://localhost:8080/adminback/api/getSpecsValue' + '?keyId=' + row.id).then(res => {\r\n        this.innerData = res.data.data\r\n        this.innerDrawer = true\r\n      })\r\n    },\r\n    openSpecs () {\r\n      this.$axios.post('http://localhost:8080/adminback/api/getSpecsKey' + '?spuCode=' + this.spuCode).then(res => {\r\n        this.gridData = res.data.data\r\n        this.drawer = true\r\n      })\r\n    },\r\n    handleClose (done) {\r\n      this.$confirm('还有未保存的工作哦确定关闭吗？')\r\n        .then(_ => {\r\n          done()\r\n        })\r\n        .catch(_ => {})\r\n    },\r\n    handleSearchSpu (row) {\r\n      this.$router.push({ path: '/product/ProductSpu', query: { categoryCode: row.categoryCode } })\r\n    },\r\n    send () {\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode).then(res => {\r\n        this.tableData = res.data.data.list\r\n        this.totalPage = res.data.data.total\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode + '&currentPage=' + this.currentPage + '&pageSize=' + val, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode + '&pageNumber=' + val + '&pageSize=' + this.pageSize, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleSearch () {\r\n    },\r\n    handleUpdateClose () {\r\n      this.dialogUpdateCategory = false\r\n    },\r\n    // 修改商品\r\n    handleUpdate (row) {\r\n      this.formUpdateCategory.categoryCode = row.categoryCode\r\n      this.formUpdateCategory.name = row.name\r\n      this.dialogUpdateCategory = true\r\n    },\r\n    // 修改商品状态\r\n    handleUpdateStatus (row) {\r\n      this.$confirm('此操作将修改与分类状态，是否继续?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        row.status = row.status === false\r\n        let productCategoryAO = {\r\n          'uniqueCode': row.uniqueCode,\r\n          'status': row.status\r\n        }\r\n        this.$axios.post('http://localhost:8080/adminback/api/updateSkuStatue', productCategoryAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('状态修改成功')\r\n          }\r\n        })\r\n      })\r\n    },\r\n    // 添加分类\r\n    handleAdd () {\r\n      this.dialogAddCategory = true\r\n      this.$axios.post('http://localhost:8080/adminback/api/getSpecs' + '?spuCode=' + this.spuCode).then(res => {\r\n        this.data1 = res.data.data\r\n      })\r\n    },\r\n    handleAddClose () {\r\n      this.$confirm('是否确认取消?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message.success('open')\r\n        this.dialogAddCategory = false\r\n      })\r\n        .catch(() => {})\r\n    },\r\n    handleAddSku () {\r\n      this.dialogAddSpecs = true\r\n    },\r\n    handleAddSpecsClose () {\r\n      this.dialogAddSpecs = false\r\n    },\r\n    handleAddSpecsSuccess () {\r\n      for (let i = 0; i < this.data1.length; i++) {\r\n        this.uniqueCodeAO[i] = { id: this.data1[i].name }\r\n      }\r\n      console.log(this.uniqueCodeAO)\r\n      this.dialogAddSpecs = false\r\n    },\r\n    // 添加sku-----------------------------------------------------------------------------------------------\r\n    handleAddSuccess () {\r\n      if (this.uniqueCodeAO.length === 0) {\r\n        this.$message.error('请先为SKU添加一个规格')\r\n      } else {\r\n        let productSkuAO = {\r\n          'spuCode': this.spuCode,\r\n          'skuCode': this.formAddCategory.skuCode,\r\n          'name': this.formAddCategory.name,\r\n          'originPrice': this.formAddCategory.originPrice,\r\n          'currentPrice': this.formAddCategory.currentPrice,\r\n          'describe': this.formAddCategory.describe,\r\n          'status': this.formUpdateCategory.status,\r\n          'uniqueCodeAO': this.uniqueCodeAO\r\n        }\r\n        this.$axios.post('http://localhost:8080/adminback/api/addProductSku', productSkuAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('添加商品成功!')\r\n            this.send()\r\n            this.dialogAddCategory = false\r\n          } else {\r\n            this.$message.error('都跟你讲编号错误了!')\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style >\r\n  .demo-table-expand {\r\n    font-size: 0;\r\n  }\r\n  .demo-table-expand label {\r\n    width: 90px;\r\n    color: #99a9bf;\r\n  }\r\n  .demo-table-expand .el-form-item {\r\n    margin-right: 0;\r\n    margin-bottom: 0;\r\n    width: 50%;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/product"}]}