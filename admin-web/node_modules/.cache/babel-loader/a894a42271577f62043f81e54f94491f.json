IAAA,aAhEA,yBAgEA,KAhEA,EAgEA,GAhEA,EAgEA;AAAA;;AACA,WAAA,QAAA,CAAA,OAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,MAAA,GAAA,eAAA,GAAA,CAAA,SAAA;AACA,QAAA,eAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA;AACA,cAAA,IAAA,EAAA,SADA;AAEA,cAAA,OAAA,EAAA;AAFA,aAAA;;AAIA,YAAA,MAAA,CAAA,SAAA;AACA;AACA,SARA;AASA,OAfA,EAeA,KAfA,CAeA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OApBA;AAqBA,KAtFA;AAuFA,IAAA,iBAvFA,6BAuFA,KAvFA,EAuFA,GAvFA,EAuFA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,KAAA,EAAA;AADA;AAFA,OAAA;AAMA;AA9FA;AA1CA,CAAA","sourcesContent":["<template>\r\n  <main>\r\n    <el-row :gutter=\"12\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :span=\"4\">\r\n              <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item>进货订单列表</el-breadcrumb-item>\r\n              </el-breadcrumb>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-card style=\"margin-top: 20px\">\r\n      <div style=\"float:left;\">\r\n        <el-form :inline=\"true\" size=\"small\" style=\"float:left;\">\r\n          <!-- 状态选择 -->\r\n          <el-form-item style=\"margin-left:20px;\">\r\n            <el-select v-model=\"status\" placeholder=\"全部订单\">\r\n              <el-option\r\n                v-for=\"item in options\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <!-- 搜索框 -->\r\n          <el-form-item>\r\n            <el-input style=\"width: 250px;\" v-model.trim.lazy=\"searchContent\" placeholder=\"请输入内容搜索\">\r\n              <i style=\"margin: 12px 5px 0 0;font-size:16px\" class=\"el-icon-search\" slot=\"suffix\"></i>\r\n            </el-input>\r\n          </el-form-item>\r\n          <!-- 查询按钮、重置按钮 -->\r\n          <el-form-item>\r\n            <el-button-group>\r\n               <el-button type=\"primary\" @click=\"handleSelect()\">查询</el-button>\r\n               <el-button @click=\"handleReset()\">重置</el-button>\r\n            </el-button-group>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <!-- 订单列表 -->\r\n      <el-table :data=\"tableData\">\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form label-position=\"left\" class=\"demo-table-expand\">\r\n              <el-form-item v-if=\"props.row.discount != null\" label=\"折扣:\">\r\n                <span>{{ props.row.discount }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.originalPrice != null\" label=\"原价:\">\r\n                <span>{{ props.row.originalPrice }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.truthPrice != null\" label=\"实付金额:\">\r\n                <span>{{ props.row.truthPrice }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.couponName != null\" label=\"优惠券:\">\r\n                <span>{{ props.row.couponName }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.cancelTime != null\" label=\"取消时间:\">\r\n                <span>{{ props.row.cancelTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.payTime != null\" label=\"付款时间:\">\r\n                <span>{{ props.row.payTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.deliverTime != null\" label=\"发货时间:\">\r\n                <span>{{ props.row.deliverTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.finishTime != null\" label=\"完成时间:\">\r\n                <span>{{ props.row.finishTime }}</span>\r\n              </el-form-item>\r\n              <el-form-item v-if=\"props.row.cancelReason != null\" label=\"取消原因:\">\r\n                <span>{{ props.row.cancelReason }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"收货地址:\">\r\n                <span>{{ props.row.address }}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"orderCode\" label=\"订单流水号\" width=\"200\" sortable></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"收货人\" width=\"100\" sortable align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"联系电话\" sortable align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"createTime\" label=\"下单时间\" width=\"170\" sortable align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"订单状态\" sortable align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"scope.row.status === -1\" style=\"color:#ff4949\">已取消</span>\r\n            <span v-else-if=\"scope.row.status === 1\" style=\"color:#ff4949\">未支付</span>\r\n            <span v-else-if=\"scope.row.status === 2\" style=\"color:#ff4949\">待发货</span>\r\n            <span v-else-if=\"scope.row.status === 3\" style=\"color:#ff4949\">待收货</span>\r\n            <span v-else-if=\"scope.row.status === 4\" style=\"color:#ff4949\">待评价</span>\r\n            <span v-else-if=\"scope.row.status === 5\" style=\"color:#ff4949\">已完成</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"订单详情\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" size=\"mini\" plain  @click=\"handleOrderDetail(scope.$index, scope.row)\">查看明细</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" fixed=\"right\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button v-if=\"scope.row.status === 1 && arr.includes('CANCEL_ORDER')\" type=\"danger\" plain size=\"mini\"  @click=\"handleCancel(scope.$index, scope.row)\">取消订单</el-button>\r\n            <el-button v-else-if=\"scope.row.status === 2 && arr.includes('DELIVER_ORDER')\" type=\"warning\" plain size=\"mini\"  @click=\"handleDeliver(scope.$index, scope.row)\">点击发货</el-button>\r\n            <el-button v-else-if=\"scope.row.status === 5\" type=\"success\" plain size=\"mini\">交易完成</el-button>\r\n            <el-button v-else type=\"info\" plain size=\"mini\">无</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页栏 -->\r\n      <div class=\"pagination\">\r\n        <el-pagination @size-change=\"handleSizeChange\"\r\n                      @current-change=\"handleCurrentChange\"\r\n                      :current-page=\"currentPage\"\r\n                      :page-sizes=\"[10, 20, 30, 40]\"\r\n                      :page-size=\"pageSize\"\r\n                      layout=\"total, sizes, prev, pager, next, jumper\"\r\n                      :total=\"count\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-card>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { reqListOrders, reqCancelOrder, reqDeliverOrder } from '../api/order.js'\r\nimport { log } from 'util'\r\nexport default {\r\n  name: 'orderList',\r\n  data () {\r\n    return {\r\n      arr:[],\r\n      count: 0,\r\n      pageSize: 10,\r\n      currentPage: 1,\r\n      status: '',\r\n      searchContent: '',\r\n      tableData: [],\r\n      options: [\r\n        {\r\n          value: '',\r\n          label: '全部订单'\r\n        }, {\r\n          value: '-1',\r\n          label: '已取消订单'\r\n        }, {\r\n          value: '1',\r\n          label: '待付款订单'\r\n        }, {\r\n          value: '2',\r\n          label: '待发货订单'\r\n        }, {\r\n          value: '3',\r\n          label: '待收货订单'\r\n        }, {\r\n          value: '4',\r\n          label: '待评价订单'\r\n        }, {\r\n          value: '5',\r\n          label: '已成交订单'\r\n        }]\r\n    }\r\n  },\r\n  created () {\r\n    this.handleShowButton()\r\n  },\r\n  mounted () {\r\n    this._loadData()\r\n  },\r\n  methods: {\r\n    handleShowButton(){\r\n      this.arr =  localStorage.getItem(\"ButtonList\");\r\n    },\r\n    _loadData () {\r\n      let params = {\r\n        pageSize: this.pageSize,\r\n        pageNum: this.currentPage,\r\n        type: 1,\r\n        status: this.status,\r\n        keyword: this.searchContent\r\n      }\r\n      reqListOrders(params).then(res => {\r\n        if (res.code === 0 && res.data != null) {\r\n          this.count = res.data.count\r\n          this.tableData = res.data.content\r\n        }\r\n      })\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this._loadData()\r\n    },\r\n    // 根据搜索框关键字查询\r\n    handleSelect () {\r\n      this.status = this.status\r\n      this._loadData()\r\n    },\r\n    handleReset () {\r\n      this.searchContent = ''\r\n      this.status = ''\r\n      this._loadData()\r\n    },\r\n    handleCancel (index, row) {\r\n      this.$confirm('是否取消订单?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        let params = {\r\n          orderCode: row.orderCode,\r\n          cancelReason: '后台取消订单'\r\n        }\r\n        reqCancelOrder(params).then(res => {\r\n          if (res.code === 0) {\r\n            this.$message({\r\n              type: 'success',\r\n              message: '取消订单成功!'\r\n            })\r\n            this._loadData()\r\n          }\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '暂不取消订单'\r\n        })\r\n      })\r\n    },\r\n    handleDeliver (index, row) {\r\n      this.$confirm('是否发货?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        let params = 'orderCode=' + row.orderCode\r\n        reqDeliverOrder(params).then(res => {\r\n          if (res.code === 0) {\r\n            this.$message({\r\n              type: 'success',\r\n              message: '发货成功!'\r\n            })\r\n            this._loadData()\r\n          }\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '暂不发货'\r\n        })\r\n      })\r\n    },\r\n    handleOrderDetail (index, row) {\r\n      this.$router.push({\r\n        path: '/layout/orderDetail',\r\n        query: {\r\n          order: row\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.pagination {\r\n    text-align: right;\r\n    margin-top: 20px;\r\n  }\r\n.demo-table-expand {\r\n    font-size: 0;\r\n  }\r\n  .demo-table-expand label {\r\n    width: 90px;\r\n    color: #99a9bf;\r\n  }\r\n  .demo-table-expand .el-form-item {\r\n    margin-right: 0;\r\n    margin-bottom: 0;\r\n    width: 50%;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}