\n        <el-form-item  :label-width=\"'70px'\" label=\"系统名称\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendSystem.name\" @blur=\"handleAddSearchName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label-width=\"'70px'\" label=\"系统标识\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendSystem.code\" @blur=\"handleAddSearchCode\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAmendSystemClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAmendSystemSuccess\">确 定</el-button>\r\n              </span>\r\n    </el-dialog>\r\n\r\n\r\n    <el-dialog\r\n      title=\"添加菜单\"\r\n      :visible.sync=\"dialogAddPermission\"\r\n      width=\"35%\">\r\n      <el-form :model=\"formAddPermission\">\r\n        <el-form-item  :label-width=\"'70px'\" label=\"菜单名称\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAddPermission.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label-width=\"'70px'\" label=\"菜单路径\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAddPermission.url\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label-width=\"'70px'\" label=\"菜单标识\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAddPermission.code\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item  label=\"所属父系统\">\r\n          <el-select v-model=\"formAddPermission.systemId\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"(item,index) in systemList\"\r\n              :key=\"index\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddPermisssionClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddPermissionSuccess\">确 定</el-button>\r\n              </span>\r\n    </el-dialog>\r\n\r\n    <el-dialog\r\n      title=\"修改菜单信息\"\r\n      :visible.sync=\"dialogAmendPermission\"\r\n      width=\"35%\">\r\n      <el-form :model=\"formAmendPermission\" ref=\"formAmendPermission\">\r\n        <el-form-item  :label-width=\"'70px'\" label=\"菜单名称\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendPermission.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label-width=\"'70px'\" label=\"菜单路径\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendPermission.url\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label-width=\"'70px'\" label=\"菜单标识\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendPermission.code\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item  label=\"所属父系统\" >\r\n          <el-select v-model=\"formAmendPermission.id\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"(item,index) in systemLists\"\r\n              :key=\"index\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAmendPermisssionClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAmendPermissionSuccess\">确 定</el-button>\r\n              </span>\r\n    </el-dialog>\r\n    <el-card class=\"box-card\" >\r\n    <el-tree\r\n      ref=\"tree\"\r\n      :data=\"treeData\"\r\n      show-checkbox\r\n      node-key=\"id\"\r\n      check-strictly\r\n      :props=\"defaultProps\"\r\n      @check=\"selectTreeNode\">\r\n    </el-tree>\r\n    </el-card>\r\n  </main>\r\n\r\n</template>\r\n\r\n<script>\r\n  import {\r\n    addPermission,\r\n    addSystem, amendPermission, amendSystem, deletePermission, deleteSystem,\r\n    isExistedSystemCode, isExistedSystemName,\r\n    listSystem,\r\n    listSystemAndPermission\r\n  } from '../../api/admin'\r\n\r\n  export default {\r\n    name: 'role',\r\n    inject:['reload'],\r\n    data () {\r\n    return {\r\n      name: '',\r\n      dialogAddSystem:false,\r\n      dialogAddPermission:false,\r\n      dialogAmendSystem:false,\r\n      dialogAmendPermission:false,\r\n      arr:[],\r\n      formAddSystem:{\r\n        name:'',\r\n        code:''\r\n      },\r\n      formAmendSystem:{\r\n        name:'',\r\n        code:''\r\n      },\r\n      formAddPermission:{\r\n        name:'',\r\n        url:'',\r\n        code:'',\r\n        systemId:''\r\n      },\r\n      formAmendPermission:{\r\n        name:'',\r\n        url:'',\r\n        id:'',\r\n        code:'',\r\n        permissionId:''\r\n      },\r\n      //全部的系统 在添加子路径使用\r\n      systemList:[],\r\n      systemLists:[],\r\n      //全部的系统和权限\r\n      treeData: [],\r\n      showAmend:{},\r\n      selectTreeOption:{},\r\n      defaultProps: {\r\n        children: 'permission',\r\n        label: 'name'\r\n      },\r\n      //默认应该选中的系统和权限\r\n      resultPermission:[]\r\n    }\r\n  },\r\n    created() {\r\n    this.handleGetAllSystemAndPermission()\r\n      this.handleAddSearchSystem()\r\n      this.handleShowButton()\r\n    },\r\n  methods: {\r\n    handleShowButton(){\r\n      this.arr =  localStorage.getItem(\"ButtonList\");\r\n    },\r\n    //让选择框变为单选\r\n    selectTreeNode:function(data,code){\r\n      if (code.checkedKeys.length > 0) {\r\n        this.$refs.tree.setCheckedKeys([data.id]);\r\n      }\r\n      this.formAmendPermission=data\r\n      this.formAmendPermission.permissionId=data.id\r\n    },\r\n    //打开新建系统按钮\r\n      handleAddSystem(){\r\n      this.dialogAddSystem=true\r\n        this.formAddSystem.name=''\r\n        this.formAddSystem.code=''\r\n    },\r\n    //打开关闭新建系统\r\n    handleAddSystemClose(){\r\n      this.dialogAddSystem=false\r\n    },\r\n    //确认新建系统\r\n    handleAddSystemSuccess(){\r\n      let SystemAO = {\r\n        name: this.formAddSystem.name,\r\n        code: this.formAddSystem.code,\r\n      }\r\n      addSystem(SystemAO).then(res=>{\r\n          if (res.code === 0){\r\n            this.$message.success('添加成功!')\r\n            this.reload()\r\n          }\r\n      })\r\n      this.dialogAddSystem=false\r\n    },\r\n    //打开新建子路径按钮\r\n    handleAddPermissson(){\r\n      this.dialogAddPermission=true\r\n    },\r\n    //关闭新建子路径\r\n    handleAddPermisssionClose(){\r\n      this.dialogAddPermission=false\r\n    },\r\n    //确认添加子路径\r\n    handleAddPermissionSuccess(){\r\n      let PermissionAO = {\r\n        name: this.formAddPermission.name,\r\n        url: this.formAddPermission.url,\r\n        code:this.formAddPermission.code,\r\n        systemId:this.formAddPermission.systemId\r\n      }\r\n      console.log(PermissionAO)\r\n      addPermission(PermissionAO).then(res=>{\r\n        if (res.code === 0){\r\n          this.$message.success('添加成功!')\r\n          this.formAddPermission.name=''\r\n          this.formAddPermission.url=''\r\n          this.formAddPermission.code=''\r\n          this.formAddPermission.systemId=''\r\n        } else{\r\n          this.$message.error(res.message)\r\n          this.formAddPermission.name=''\r\n          this.formAddPermission.url=''\r\n          this.formAddPermission.code=''\r\n          this.formAddPermission.systemId=''\r\n        }\r\n      })\r\n      this.dialogAddPermission=false\r\n\r\n    },\r\n    //删除节点\r\n    handleDelete(){\r\n      let temp=this.$refs.tree.getCheckedKeys().concat(this.$refs.tree.getHalfCheckedKeys())\r\n      let flag=false\r\n      if(temp.length<=0) {\r\n        alert(\"请选择要删除的节点\")\r\n        return;\r\n      }\r\n      for (var j=0;j<this.systemList.length;j++){\r\n        if(temp[0] === this.systemList[j].id){\r\n          flag=true\r\n          if(this.treeData[j].permission.length>0){\r\n              alert(\"当前父节点不为空不能删除！\")\r\n              return;\r\n            }else {\r\n              this.$confirm('是否删除该系统？','提示',{\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n              }).then(()=>{\r\n                let DeleteSystemAO={\r\n                  systemId:temp[0]\r\n                }\r\n                console.log(DeleteSystemAO)\r\n                deleteSystem(DeleteSystemAO).then(res => {\r\n                  if (res.code === 0) {\r\n                    this.$message.success('删除成功!')\r\n                    this.reload();\r\n                  } else {\r\n                    this.$message.error(res.message)\r\n                    return;\r\n                  }\r\n                })\r\n              }).catch(()=>{\r\n                this.$message.info('已取消修改')\r\n                return;\r\n              })\r\n            }\r\n          }\r\n        }\r\n      if(flag === false){\r\n        this.$confirm('是否删除该子菜单？','提示',{\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(()=>{\r\n          let DeleteSystemAO={\r\n            permissionId:temp[0]\r\n          }\r\n          console.log(DeleteSystemAO)\r\n          deletePermission(DeleteSystemAO).then(res => {\r\n            if (res.code === 0) {\r\n              this.$message.success('删除成功!')\r\n            } else {\r\n              this.$message.error(res.message)\r\n            }\r\n          })\r\n        }).catch(()=>{\r\n          this.$message.info('已取消修改')\r\n        })\r\n      }\r\n    },\r\n    //修改节点\r\n    handleAmend(){\r\n      let temp=this.$refs.tree.getCheckedKeys().concat(this.$refs.tree.getHalfCheckedKeys())\r\n      let flag=false\r\n      if(temp.length<=0){\r\n        alert(\"请选择要修改的节点\")\r\n        return;\r\n      }\r\n      for (var j=0;j<this.systemList.length;j++){\r\n        if(temp[0] === this.systemList[j].id){\r\n          flag=true\r\n          this.dialogAmendSystem=true\r\n          this.formAmendSystem=this.systemList[j]\r\n        }\r\n      }\r\n      if(flag === false){\r\n        this.dialogAmendPermission=true\r\n        this.formAmendPermission.id=parseInt(temp[0]%100)\r\n      }\r\n    },\r\n    //确认修改系统\r\n    handleAmendSystemSuccess(){\r\n      this.dialogAmendSystem=false\r\n      let SystemAO = {\r\n        systemId:this.formAmendSystem.id,\r\n        name: this.formAmendSystem.name,\r\n        code: this.formAmendSystem.code,\r\n      }\r\n      console.log(SystemAO)\r\n      amendSystem(SystemAO).then(res=>{\r\n        if (res.code === 0){\r\n          this.$message.success('修改成功!')\r\n          this.reload()\r\n        } else{\r\n          this.$message.error(res.message)\r\n        }\r\n      })\r\n    },\r\n    //关闭修改系统\r\n    handleAmendSystemClose(){\r\n      this.dialogAmendSystem=false\r\n    },\r\n    //关闭修改子路径\r\n    handleAmendPermisssionClose(){\r\n      this.dialogAmendPermission=false\r\n    },\r\n    //确认修改子菜单\r\n    handleAmendPermissionSuccess(){\r\n      let PermissionAO = {\r\n        permissionId:this.formAmendPermission.permissionId,\r\n        name: this.formAmendPermission.name,\r\n        url: this.formAmendPermission.url,\r\n        code: this.formAmendPermission.code,\r\n        systemId:this.formAmendPermission.id\r\n      }\r\n      console.log(PermissionAO)\r\n      amendPermission(PermissionAO).then(res=>{\r\n        if (res.code === 0){\r\n          this.$message.success('修改成功!')\r\n          this.reload()\r\n        } else{\r\n          this.$message.error(res.message)\r\n        }\r\n      })\r\n      this.dialogAmendPermission=false\r\n    },\r\n    //添加时搜索名称是否存在\r\n    handleAddSearchName(){\r\n      let SystemAO = {\r\n        name: this.formAddSystem.name\r\n      }\r\n      isExistedSystemName(SystemAO).then(res => {\r\n        if (res.code === 9999) {\r\n          this.$message.error('该系统名称已经存在！')\r\n          this.formAddSystem.name=''\r\n        }\r\n      }).catch(err=>{\r\n        this.$message.error('无权限访问！')\r\n      })\r\n    },\r\n    //添加时搜索标识是否存在\r\n    handleAddSearchCode(){\r\n      let SystemAO = {\r\n        code: this.formAddSystem.code\r\n      }\r\n      isExistedSystemCode(SystemAO).then(res => {\r\n        if (res.code === 9999) {\r\n          this.$message.error('该系统标识已经存在！')\r\n          this.formAddSystem.code=''\r\n        }\r\n      }).catch(err=>{\r\n        this.$message.error('无权限访问！')\r\n      })\r\n    },\r\n    //获取所有的系统和权限\r\n    handleGetAllSystemAndPermission(){\r\n      listSystemAndPermission().then(res=>{\r\n        this.treeData=res.data\r\n      }).catch(err=>{\r\n        this.$message.error('无权限访问！')\r\n      })\r\n    },\r\n    //添加子路径时渲染下拉框\r\n    handleAddSearchSystem(){\r\n      listSystem().then(res=>{\r\n        this.systemList=res.data\r\n        this.systemLists=res.data\r\n      }).catch(err=>{\r\n        this.$message.error('无权限访问！')\r\n      })\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/admin"}]}