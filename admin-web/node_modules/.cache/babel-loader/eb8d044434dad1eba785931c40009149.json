             clearable\r\n              placeholder=\"请输入分类名\"\r\n              prefix-icon=\"el-icon-search\"\r\n              v-model=\"name\">\r\n            </el-input>\r\n          </el-col>\r\n          <el-col :offset='15' :span=\"1\">\r\n            <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n          </el-col>\r\n          <el-col :offset='1' :span=\"1\">\r\n            <el-button @click=\"handleAdd\" type=\"primary\">添加</el-button>\r\n          </el-col>\r\n          <!--添加分类-->\r\n          <el-dialog\r\n            title=\"添加分类\"\r\n            :visible.sync=\"dialogAddCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddCategory\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"分类编号\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.categoryCode\" @blur=\"check\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"分类名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.name\" ></el-input>\r\n              </el-form-item>\r\n              <el-input  v-model=\"formAddCategory.parentCode\" :value= \"0\" type=\"hidden\"></el-input>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formAddCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--修改分类-->\r\n          <el-dialog\r\n            title=\"修改分类\"\r\n            :visible.sync=\"dialogUpdateCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formUpdateCategory\">\r\n              <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.categoryCode\" type=\"hidden\"></el-input>\r\n              <el-form-item :label-width=\"'100px'\" label=\"分类名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.name\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formUpdateCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleUpdateClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleUpdateSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--添加品牌-->\r\n          <el-dialog\r\n            title=\"添加品牌\"\r\n            :visible.sync=\"dialogAddBrand\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddBrand\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"品牌名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddBrand.name\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"品牌描述\">\r\n                <el-input type=\"textarea\" autocomplete=\"off\" v-model=\"formAddBrand.describe\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"品牌编号\">\r\n                <el-input type=\"textarea\" autocomplete=\"off\" v-model=\"formAddBrand.code\" @blur=\"checkBrand\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formAddBrand.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddBrandClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddBrandSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--数据表格-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\"\r\n        tooltip-effect=\"dark\">\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"分类编号\">\r\n          <template slot-scope=\"scope\">{{ scope.row.categoryCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"分类名\">\r\n          <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"当前状态\"\r\n          prop=\"status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button :type=\"scope.row.status===true?'primary':'danger'\"\r\n                       plain size=\"small\"\r\n                       @click=\"handleUpdateStatus(scope.row)\">\r\n              {{ scope.row.status === true ? '正常' : '不启用' }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleUpdate(scope.row)\">编辑</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"其他\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleAddBrand(scope.row)\">添加品牌</el-button>\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleSearchSpu(scope.row)\">查看商品</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-col>\r\n  </el-row>\r\n  <!--分页-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :offset=\"10\" :span=\"12\">\r\n            <div class=\"block\">\r\n              <el-pagination\r\n                :current-page=currentPage\r\n                :page-size=pageSize\r\n                :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                :total=totalPage\r\n                @current-change=\"handleCurrentChange\"\r\n                @size-change=\"handleSizeChange\"\r\n                background\r\n                layout=\"total, sizes, prev, pager, next, jumper\">\r\n              </el-pagination>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n</main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  mounted () {\r\n    this.send()\r\n  },\r\n  name: 'ProductCategory',\r\n  data () {\r\n    return {\r\n      name: '',\r\n      dialogUpdateCategory: false,\r\n      dialogAddCategory: false,\r\n      dialogAddBrand: false,\r\n      formUpdateCategory: {\r\n        categoryCode: '',\r\n        name: '',\r\n        status: true\r\n      },\r\n      formAddBrand: {\r\n        code: '',\r\n        categoryCode: '',\r\n        name: '',\r\n        describe: '',\r\n        status: true\r\n      },\r\n      formAddCategory: {\r\n        categoryCode: '',\r\n        name: '',\r\n        parentCode: this.$route.query.categoryCode,\r\n        status: true\r\n      },\r\n      parentCategoryList: [\r\n        {\r\n          code: 0,\r\n          name: '分类A'\r\n        },\r\n        {\r\n          code: 1,\r\n          name: '分类B'\r\n        }\r\n      ],\r\n      // 数据表格\r\n      tableData: [],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  methods: {\r\n    handleSearchSpu (row) {\r\n      this.$router.push({ path: '/product/Spu', query: { categoryCode: row.categoryCode } })\r\n    },\r\n    checkBrand () {\r\n      let check = this.formAddBrand.code\r\n      this.$axios.post('http://localhost:8080/adminback/api/checkBrandCode' + '?brandCode=' + check).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$message({\r\n            message: '编号重复',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '编号可使用',\r\n            type: 'success'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    check () {\r\n      let check = this.formAddCategory.categoryCode\r\n      this.$axios.post('http://localhost:8080/adminback/api/checkExist' + '?categoryCode=' + check).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$message({\r\n            message: '编号重复',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '编号可使用',\r\n            type: 'success'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    send () {\r\n      this.$axios.post('http://localhost:8080/adminback/api/getCategoryChildren' + '?parentCode=' + this.$route.query.categoryCode).then(res => {\r\n        this.tableData = res.data.data.list\r\n        this.totalPage = res.data.data.total\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleAddBrand (row) {\r\n      this.dialogAddBrand = true\r\n      this.formAddBrand.categoryCode = row.categoryCode\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n      this.$axios.post('http://localhost:8080/adminback/api/getCategoryChildren' + '?parentCode=' + this.$route.query.categoryCode + '&currentPage=' + this.currentPage + '&pageSize=' + val, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleAddBrandSuccess () {\r\n      let productBrandAO = {\r\n        'categoryCode': this.formAddBrand.categoryCode,\r\n        'code': this.formAddBrand.code,\r\n        'describe': this.formAddBrand.describe,\r\n        'status': this.formAddBrand.status,\r\n        'name': this.formAddBrand.name\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/addBrand', productBrandAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加品牌成功!')\r\n          this.dialogAddBrand = false\r\n        } else {\r\n          this.$message.error('都跟你讲编号错误了!')\r\n        }\r\n      })\r\n    },\r\n    handleAddBrandClose () {\r\n      this.dialogAddBrand = false\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.$axios.post('http://localhost:8080/adminback/api/getCategoryChildren' + '?parentCode=' + this.$route.query.categoryCode + '&pageNumber=' + val + '&pageSize=' + this.pageSize, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleUpdateSuccess () {\r\n      let productCategoryAO = {\r\n        'categoryCode': this.formUpdateCategory.categoryCode,\r\n        'name': this.formUpdateCategory.name,\r\n        'status': this.formUpdateCategory.status\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/updateCategory', productCategoryAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('修改成功')\r\n          this.send()\r\n          this.dialogUpdateCategory = false\r\n        } else {\r\n          this.$message.error('未知错误')\r\n        }\r\n      })\r\n    },\r\n    handleSearch () {\r\n    },\r\n    handleUpdateClose () {\r\n      this.dialogUpdateCategory = false\r\n    },\r\n    // 修改商品\r\n    handleUpdate (row) {\r\n      this.formUpdateCategory.categoryCode = row.categoryCode\r\n      this.formUpdateCategory.name = row.name\r\n      this.dialogUpdateCategory = true\r\n    },\r\n    // 修改商品状态\r\n    handleUpdateStatus (row) {\r\n      this.$confirm('此操作将修改与分类状态，是否继续?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        row.status = row.status === false\r\n        let productCategoryAO = {\r\n          'categoryCode': row.categoryCode,\r\n          'status': row.status\r\n        }\r\n        this.$axios.post('http://localhost:8080/adminback/api/updateCategory', productCategoryAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('状态修改成功')\r\n          }\r\n        })\r\n      })\r\n    },\r\n    // 添加分类\r\n    handleAdd () {\r\n      this.dialogAddCategory = true\r\n    },\r\n    handleAddClose () {\r\n      this.$confirm('是否确认取消?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message.success('open')\r\n        this.dialogAddCategory = false\r\n      })\r\n        .catch(() => {})\r\n    },\r\n    handleAddSuccess () {\r\n      let categoryAO = {\r\n        'categoryCode': this.formAddCategory.categoryCode,\r\n        'name': this.formAddCategory.name,\r\n        'parentCode': this.formAddCategory.parentCode,\r\n        'status': this.formAddCategory.status\r\n      }\r\n      this.$axios.post('http://localhost:8080/adminback/api/addCategory', categoryAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加商品成功!')\r\n          this.send()\r\n          this.dialogAddCategory = false\r\n        } else {\r\n          this.$message.error('都跟你讲编号错误了!')\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/product"}]}