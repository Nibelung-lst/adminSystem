           <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                <el-breadcrumb-item>系统列表</el-breadcrumb-item>\r\n              </el-breadcrumb>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--搜索栏-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :span=\"5\">\r\n              <el-input\r\n                clearable\r\n                placeholder=\"系统名称\"\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"name\">\r\n              </el-input>\r\n            </el-col>\r\n            <el-col :offset='15' :span=\"1\">\r\n              <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n            </el-col>\r\n            <el-col :offset='1' :span=\"1\">\r\n              <el-button @click=\"handleAdd\" type=\"primary\">添加</el-button>\r\n            </el-col>\r\n            <el-dialog\r\n              title=\"添加系统\"\r\n              :visible.sync=\"dialogAddRole\"\r\n              width=\"30%\">\r\n              <el-form :model=\"formAddRole\" ref=\"formAddRole\">\r\n                <el-form-item :label-width=\"'100px'\" label=\"系统名称\">\r\n                  <el-input autocomplete=\"off\" v-model=\"formAddRole.name\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item :label-width=\"'100px'\" label=\"系统标识\">\r\n                  <el-input autocomplete=\"off\" v-model=\"formAddRole.code\"></el-input>\r\n                </el-form-item>\r\n              </el-form>\r\n              <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n            </el-dialog>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--数据表格-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-table\r\n          :data=\"tableData\"\r\n          border\r\n          style=\"width: 100%\"\r\n          tooltip-effect=\"dark\">\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"系统id\">\r\n            <template slot-scope=\"scope\">{{ scope.row.id }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"系统标识\">\r\n            <template slot-scope=\"scope\">{{ scope.row.code }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"系统名称\">\r\n            <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"创建时间\">\r\n            <template slot-scope=\"scope\">{{ scope.row.createTime }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"primary\" plain size=\"small\" @click=\"handleAmend(scope.row)\">编辑</el-button>\r\n              <el-button type=\"danger\" plain size=\"small\"  @click=\"handleDelete(scope.row)\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-dialog\r\n      title=\"修改系统信息\"\r\n      :visible.sync=\"dialogAmendSystem\"\r\n      width=\"35%\">\r\n      <el-form :model=\"formAmendSystem\">\r\n        <el-form-item :label-width=\"'100px'\" label=\"修改名称\">\r\n          <el-input autocomplete=\"off\" v-model=\"formAmendSystem.name\" ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAmendClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAmendSuccess\">确 定</el-button>\r\n              </span>\r\n    </el-dialog>\r\n    <!--分页-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :offset=\"10\" :span=\"12\">\r\n              <div class=\"block\">\r\n                <el-pagination\r\n                  :current-page=currentPage\r\n                  :page-size=pageSize\r\n                  :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                  :total=totalPage\r\n                  @current-change=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { addSystem, amendSystemName, deleteSystem, findBySystemName, listSystem } from '../../api/admin'\r\n\r\nexport default {\r\n  name: 'role',\r\n  data () {\r\n    return {\r\n      name: '',\r\n      dialogAmendSystem: false,\r\n      formAmendSystem: {\r\n        name: ''\r\n      },\r\n      dialogAddRole: false,\r\n      formAddRole: {\r\n        name: '',\r\n        code: ''\r\n      },\r\n      amendSystemId: '',\r\n      // 数据表格\r\n      tableData: [\r\n        {\r\n          code: 1,\r\n          name: 'orderManager',\r\n          status: 1,\r\n          parentCode: '订单管理员'\r\n        }\r\n      ],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  created () {\r\n    this.handleGetAllSystem()\r\n  },\r\n  methods: {\r\n    handleDelete (num) {\r\n      let systemAO = {\r\n        systemId: num.id\r\n      }\r\n      this.$confirm('是否删除该系统？', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        deleteSystem(systemAO).then(res => {\r\n          if (res.code === 0) {\r\n            this.$message.success('删除成功!')\r\n          } else {\r\n            this.$message.error(res.message)\r\n          }\r\n        })\r\n      }).catch(() => {\r\n        this.$message.info('已取消修改')\r\n      })\r\n    },\r\n    handleAmend (num) {\r\n      this.dialogAmendSystem = true\r\n      this.amendSystemId = num.id\r\n    },\r\n    // 关闭修改\r\n    handleAmendClose () {\r\n      this.dialogAmendSystem = false\r\n    },\r\n    handleAmendSuccess () {\r\n      let amendSystemAO = {\r\n        systemId: this.amendSystemId,\r\n        systemName: this.formAmendSystem.name\r\n      }\r\n      amendSystemName(amendSystemAO).then(res => {\r\n        if (res.code === 0) {\r\n          this.$message.success('修改成功!')\r\n        } else {\r\n          this.$message.error(res.message)\r\n        }\r\n      })\r\n      this.dialogAmendSystem = false\r\n    },\r\n\r\n    handleSearch () {\r\n      let SystemAO = {\r\n        systemName: this.name\r\n      }\r\n      console.log(this.name)\r\n      findBySystemName(SystemAO).then(res => {\r\n        this.tableData = res.data\r\n      })\r\n    },\r\n    // 打开添加按钮\r\n    handleAdd () {\r\n      this.dialogAddRole = true\r\n    },\r\n    // 取消添加\r\n    handleAddClose () {\r\n      this.dialogAddRole = false\r\n    },\r\n    // 确认添加\r\n    handleAddSuccess () {\r\n      let AddSystemAO = {\r\n        systemCode: this.formAddRole.code,\r\n        systemName: this.formAddRole.name\r\n      }\r\n      addSystem(AddSystemAO).then(res => {\r\n        if (res.code === 0) {\r\n          this.$message.success('添加成功!')\r\n        } else {\r\n          this.$message.error(res.message)\r\n        }\r\n      })\r\n      this.$refs.formAddRole.resetFields()\r\n      this.dialogAddRole = false\r\n    },\r\n    // TODO 获取所有系统\r\n    handleGetAllSystem () {\r\n      listSystem().then(res => {\r\n        this.tableData = res.data\r\n      })\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}