ow>\r\n    <!--搜索栏-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row>\r\n            <el-col>\r\n              <el-form :inline=\"true\" :model=\"queryParams\" size=\"small\" label-width=\"140px\">\r\n                <el-form-item label=\"优惠券名称：\">\r\n                  <el-input v-model=\"queryParams.name\" class=\"input-width\" placeholder=\"优惠券名称\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"优惠券类型：\">\r\n                  <el-select v-model=\"queryParams.useType\" placeholder=\"全部\" clearable class=\"input-width\">\r\n                    <el-option v-for=\"item in typeOptions\"\r\n                               :key=\"item.value\"\r\n                               :label=\"item.label\"\r\n                               :value=\"item.value\">\r\n                    </el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"优惠券状态：\">\r\n                  <el-select v-model=\"queryParams.status\" placeholder=\"全部\" clearable class=\"input-width\">\r\n                    <el-option v-for=\"item in statusOptions\"\r\n                               :key=\"item.value\"\r\n                               :label=\"item.label\"\r\n                               :value=\"item.value\">\r\n                    </el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                  <el-button @click=\"handleSearch\" type=\"primary\">查询</el-button>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                  <el-button @click=\"handleAdd()\" type=\"primary\">添加优惠券</el-button>\r\n               <!--   <el-button type=\"primary\" plain size=\"small\" @click=\"handleView()\">查看流水</el-button>-->\r\n                </el-form-item>\r\n              </el-form>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--数据表格-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-table\r\n          :data=\"couponList\"\r\n          style=\"width: 100%\"\r\n          tooltip-effect=\"dark\">\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"编号\">\r\n            <template slot-scope=\"scope\">{{ scope.row.id }}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"优惠劵名称\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.name}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"类型\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.useType | formatUseType}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"可使用商品\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.useRangeName}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"使用门槛\" width=\"140\" align=\"center\">\r\n            <template slot-scope=\"scope\">满{{scope.row.minAmount}}元可用</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"面值\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.amount}}元</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"领取类型\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.sendType | formatType}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"有效期\" width=\"180\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.startTime |formatDate}}至{{scope.row.endTime |formatDate}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"上下线\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.status | formatStatus}}</template>\r\n          </el-table-column>\r\n          <el-table-column label=\"状态\" width=\"100\" align=\"center\">\r\n            <template slot-scope=\"scope\">{{scope.row.endTime | formatExpire}}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"primary\" plain size=\"small\">编辑</el-button>\r\n              <el-button type=\"primary\" plain size=\"small\" @click=\"handleView(scope.$index,scope.row)\">查看流水</el-button>\r\n              <el-button type=\"danger\" plain size=\"small\">删除</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n    <el-dialog>\r\n\r\n    </el-dialog>\r\n    <!--分页-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :offset=\"10\" :span=\"12\">\r\n              <div class=\"block\">\r\n                <el-pagination\r\n                  :current-page=currentPage\r\n                  :page-size=pageSize\r\n                  :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                  :total=total\r\n                  @current-change=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAllCoupon } from '@/api/marketing'\r\nimport { formatDate } from '@/utils/date'\r\n\r\nexport default {\r\n  created () {\r\n    this.handleGetAllCoupon()\r\n  },\r\n  mounted () {\r\n  },\r\n  filters: {\r\n    formatType (type) {\r\n      if (type === 1) {\r\n        return '用户可领取'\r\n      } else if (type === 2) {\r\n        return '仅后台赠送'\r\n      } else {\r\n        return '购物赠送'\r\n      }\r\n    },\r\n    formatUseType (useType) {\r\n      if (useType === 1) {\r\n        return '全场通用'\r\n      } else if (useType === 2) {\r\n        return '指定分类'\r\n      } else {\r\n        return '指定商品'\r\n      }\r\n    },\r\n    formatDate (time) {\r\n      if (time == null || time === '') {\r\n        return 'N/A'\r\n      }\r\n      let date = new Date(time)\r\n      return formatDate(date, 'yyyy-MM-dd mm:ss')\r\n    },\r\n    formatStatus (status) {\r\n      if (status === 1) {\r\n        return '已上线'\r\n      } else {\r\n        return '下线'\r\n      }\r\n    },\r\n    formatExpire (endTime) {\r\n      let now = new Date().getTime()\r\n      let endDate = new Date(endTime)\r\n      if (endDate > now) {\r\n        return '未过期'\r\n      } else {\r\n        return '已过期'\r\n      }\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      useTypeOptions: [\r\n        {\r\n          label: '全品类',\r\n          value: 1\r\n        },\r\n        {\r\n          label: '指定分类',\r\n          value: 2\r\n        },\r\n        {\r\n          label: '指定商品',\r\n          value: 3\r\n        }\r\n      ],\r\n      typeOptions: [\r\n        {\r\n          label: '用户可领取',\r\n          value: 1\r\n        },\r\n        {\r\n          label: '仅后台赠送',\r\n          value: 2\r\n        },\r\n        {\r\n          label: '购物赠送',\r\n          value: 3\r\n        }\r\n      ],\r\n      statusOptions: [\r\n        {\r\n          label: '已发布',\r\n          value: 1\r\n        },\r\n        {\r\n          label: '下线',\r\n          value: 2\r\n        }\r\n      ],\r\n      // 搜索栏\r\n      name: '',\r\n      // 优惠券数组\r\n      couponList: [],\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 5,\r\n        name: '',\r\n        type: 1,\r\n        status: null\r\n      },\r\n      addCouponForm: {},\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总数\r\n      total: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  methods: {\r\n    handleView (index, row) {\r\n      console.log(row.id)\r\n      // this.$router.push(\"/marketing/couponHistory\")\r\n      this.$router.push({ path: '/marketing/couponHistory', query: { id: row.id } })\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.handleGetAllCoupon()\r\n    },\r\n    // 查询按钮\r\n    handleSearch () {\r\n      this.currentPage = 1\r\n      this.handleGetAllCoupon()\r\n    },\r\n    // 修改商品状态\r\n    handleUpdateStatus (row) {\r\n      row.status = row.status === 1 ? 0 : 1\r\n    },\r\n\r\n    // 获取优惠券\r\n    handleGetAllCoupon () {\r\n      this.queryParams.pageNum = this.currentPage\r\n      this.queryParams.pageSize = this.pageSize\r\n      getAllCoupon(this.queryParams).then(res => {\r\n        if (res.code === 0) {\r\n          this.couponList = res.data.content\r\n          this.total = res.data.count\r\n        }\r\n      })\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .input-width {\r\n    width: 203px;\r\n  }\r\n</style>\r\n"]}]}