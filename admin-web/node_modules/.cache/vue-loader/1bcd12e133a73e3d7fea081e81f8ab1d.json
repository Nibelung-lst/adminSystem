         <el-col :span=\"4\">\r\n              <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n                <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n               <!-- <el-breadcrumb-item>优惠券管理</el-breadcrumb-item>-->\r\n                <el-breadcrumb-item>赠送优惠券</el-breadcrumb-item>\r\n              </el-breadcrumb>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--搜索栏-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :span=\"4\">\r\n              <el-input\r\n                clearable\r\n                placeholder=\"请输入用户名\"\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"searchData.name\">\r\n              </el-input>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-select :span=\"4\" v-model=\"searchData.sex\" clearable placeholder=\"请选择性别\" value=\"\">\r\n                <el-option\r\n                  v-for=\"item in sex\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-select :span=\"4\" v-model=\"searchData.status\" clearable placeholder=\"请选择状态\" value=\"\">\r\n                <el-option\r\n                  v-for=\"item in status\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n            <el-col :span=\"4\">\r\n              <el-select :span=\"4\" v-model=\"searchData.levelName\" clearable placeholder=\"请选择等级\" value=\"\">\r\n                <el-option\r\n                  v-for=\"item in levelName\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n            <el-col :offset='3' :span=\"1\">\r\n              <el-button @click=\"handleSearchUser\" type=\"primary\">查询</el-button>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n    <!--数据表格-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-table\r\n          :data=\"tableData\"\r\n          border\r\n          style=\"width: 100%\"\r\n          tooltip-effect=\"dark\">\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"id\">\r\n            <template slot-scope=\"scope\">{{ scope.row.id }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"头像\"\r\n            width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-image\r\n                :src=\"scope.row.headImgPath\"\r\n                :fit=\"'fit'\"\r\n                style=\"width: 60px;\">\r\n                <div class=\"image-slot\" slot=\"error\">\r\n                  <el-image :fit=\"'fit'\"\r\n                            :src=\"scope.row.headImgPath\"\r\n                            style=\"width: 60px;\">\r\n                  </el-image>\r\n                </div>\r\n              </el-image>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"用户名\">\r\n            <template slot-scope=\"scope\">{{ scope.row.userName }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"昵称\">\r\n            <template slot-scope=\"scope\">{{ scope.row.nickName }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"等级\">\r\n            <template slot-scope=\"scope\">{{ scope.row.levelName }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"性别\">\r\n            <template slot-scope=\"scope\">\r\n              <i :class=\"scope.row.sex === 1?'el-icon-male':'el-icon-female'\"></i>\r\n              {{ scope.row.sex === 1 ? '男':'女'}}\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"总积分\">\r\n            <template slot-scope=\"scope\">{{ scope.row.points }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"总消费\">\r\n            <template slot-scope=\"scope\">{{ scope.row.totalConsumption }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"生日\">\r\n            <template slot-scope=\"scope\">{{ scope.row.birthday }}</template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"当前状态\"\r\n            prop=\"status\">\r\n            <template slot-scope=\"scope\">\r\n                {{ scope.row.status === 1 ? '正常' : '已禁用' }}\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column\r\n            align=\"center\"\r\n            label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"primary\" plain size=\"small\" @click=\"handleSend(scope.$index,scope.row)\">赠送优惠券</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n    <!--分页-->\r\n    <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"always\">\r\n          <el-row :gutter=\"12\">\r\n            <el-col :offset=\"10\" :span=\"12\">\r\n              <div class=\"block\">\r\n                <el-pagination\r\n                  :current-page=currentPage\r\n                  :page-size=pageSize\r\n                  :page-sizes=\"[5]\"\r\n                  :total=totalPage\r\n                  @current-change=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n<script>\r\n    import {searchUser } from '@/api/user'\r\n    import {sendUserCoupon} from '@/api/marketing'\r\n    export default {\r\n        created () {\r\n            this.handleSearchUser()\r\n            this.handleShowButton()\r\n        },\r\n        mounted () {\r\n\r\n        },\r\n        data () {\r\n            return {\r\n                // 搜索栏\r\n                searchData: {\r\n                    name: '',\r\n                    sex: '',\r\n                    status: '',\r\n                    levelName: '',\r\n                    pageNum: 1,\r\n                    pageSize: 5\r\n                },\r\n                name: '',\r\n                sex: [\r\n                    {\r\n                        value: 1,\r\n                        label: '男'\r\n                    },\r\n                    {\r\n                        value: 2,\r\n                        label: '女'\r\n                    }\r\n                ],\r\n                status: [\r\n                    {\r\n                        value: 1,\r\n                        label: '正常'\r\n                    },\r\n                    {\r\n                        value: 0,\r\n                        label: '禁用'\r\n                    }\r\n                ],\r\n                levelName: [\r\n                    {\r\n                        value: '青铜',\r\n                        label: '青铜'\r\n                    },\r\n                    {\r\n                        value: '白银',\r\n                        label: '白银'\r\n                    },\r\n                    {\r\n                        value: '黄金',\r\n                        label: '黄金'\r\n                    },\r\n                    {\r\n                        value: '钻石',\r\n                        label: '钻石'\r\n                    },\r\n                    {\r\n                        value: '星耀',\r\n                        label: '星耀'\r\n                    }\r\n                ],\r\n                // 数据\r\n                tableData: [\r\n                    {\r\n                        id: '1',\r\n                        levelId: 1,\r\n                        levelName: '青铜',\r\n                        userName: '12323434556',\r\n                        nickName: '传说中的勇者',\r\n                        headImgPath: 'https://fuss10.elemecdn.com/e/5d/4a731a90594a4af544c0c25941171jpeg.jpeg',\r\n                        status: 1,\r\n                        sex: 1,\r\n                        birthday: '2019-9-12',\r\n                        points: '122',\r\n                        totalConsumption: '1000.00',\r\n                        createTime: '2019-10-23',\r\n                        updateTime: '2020-12-09'\r\n                    }\r\n                ],\r\n                // 每页几条数据\r\n                pageSize: 5,\r\n                // 总页数\r\n                totalPage: 0,\r\n                // 当前页\r\n                currentPage: 1,\r\n                arr:[]\r\n            }\r\n        },\r\n        methods: {\r\n            handleShowButton(){\r\n                this.arr =  localStorage.getItem(\"ButtonList\");\r\n            },\r\n            // 每页几条数据变化\r\n            handleSizeChange (val) {\r\n                this.pageSize = val\r\n            },\r\n            // 当前页变化\r\n            handleCurrentChange (val) {\r\n                this.currentPage = val\r\n                this.searchData.pageNum = this.currentPage\r\n                this.searchData.pageSize = this.pageSize\r\n                this.handleSearchUser()\r\n            },\r\n            // 初始化数据\r\n            // 模糊查询用户信息\r\n            handleSearchUser () {\r\n                searchUser(this.searchData).then(res => {\r\n                    if (res.code === 0) {\r\n                        this.$message.success('成功')\r\n                        this.totalPage = res.data.count\r\n                        this.tableData = res.data.content\r\n                    } else {\r\n                        this.$message.error('失败')\r\n                    }\r\n                })\r\n            },\r\n            handleSend(index,row){\r\n                let params=\"userId=\"+row.id+\"&userName=\"+row.userName+\"&couponCode=\"+this.$route.query.couponCode\r\n                console.log(params)\r\n                sendUserCoupon(params).then(res=>{\r\n                    if (res.code === 0) {\r\n                        this.$message.success('赠送成功')\r\n                    } else {\r\n                        this.$message.error('失败')\r\n                    }\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n  .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n\r\n  .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n\r\n  .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n\r\n  .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n\r\n  .el-upload--picture-card {\r\n    background-color: #fbfdff;\r\n    border: 1px dashed #c0ccda;\r\n    border-radius: 6px;\r\n    -webkit-box-sizing: border-box;\r\n    box-sizing: border-box;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 176px;\r\n    vertical-align: top;\r\n  }\r\n</style>\r\n"]}]}