tle=\"添加SKU\"\r\n            :visible.sync=\"dialogAddCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formAddCategory\">\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku名字\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.name\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku编号\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.skuCode\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku描述\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.describe\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku原价\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.originPrice\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item :label-width=\"'100px'\" label=\"Sku现价\">\r\n                <el-input autocomplete=\"off\" v-model=\"formAddCategory.currentPrice\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-button type=\"primary\" @click=\"handleAddSku\">添加规格</el-button>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formAddCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleAddClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleAddSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n          <!--修改分类-->\r\n          <el-dialog\r\n            title=\"修改分类\"\r\n            :visible.sync=\"dialogUpdateCategory\"\r\n            width=\"30%\">\r\n            <el-form :model=\"formUpdateCategory\">\r\n              <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.categoryCode\" type=\"hidden\"></el-input>\r\n              <el-form-item :label-width=\"'100px'\" label=\"分类名\">\r\n                <el-input autocomplete=\"off\" v-model=\"formUpdateCategory.name\" ></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                <el-switch\r\n                  style=\"display: block\"\r\n                  v-model=\"formUpdateCategory.status\"\r\n                  active-color=\"#13ce66\"\r\n                  inactive-color=\"#ff4949\"\r\n                  active-text=\"启用\"\r\n                  inactive-text=\"不启用\">\r\n                </el-switch>\r\n              </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"handleUpdateClose\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"handleUpdateSuccess\">确 定</el-button>\r\n              </span>\r\n          </el-dialog>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n  <!--数据表格-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\"\r\n        tooltip-effect=\"dark\">\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n              <el-form-item label=\"商品名称\">\r\n                <span>{{ props.row.name }}</span>\r\n              </el-form-item>\r\n              <el-form-item label=\"所属店铺\">\r\n                <span>{{ props.row.shop }}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"唯一编码\">\r\n          <template slot-scope=\"scope\">{{ scope.row.uniqueCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU编号\">\r\n          <template slot-scope=\"scope\">{{ scope.row.skuCode }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU名\">\r\n          <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU描述\">\r\n          <template slot-scope=\"scope\">{{ scope.row.describe }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU初始价格\">\r\n          <template slot-scope=\"scope\">{{ scope.row.originPrice }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"SKU现价\">\r\n          <template slot-scope=\"scope\">{{ scope.row.currentPrice }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"当前状态\"\r\n          prop=\"status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button :type=\"scope.row.status===true?'primary':'danger'\"\r\n                       plain size=\"small\"\r\n                       @click=\"handleUpdateStatus(scope.row)\">\r\n              {{ scope.row.status === true ? '正常' : '不启用' }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleUpdate(scope.row)\">编辑</el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          align=\"center\"\r\n          label=\"其他\">\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n    </el-col>\r\n  </el-row>\r\n  <!--分页-->\r\n  <el-row :gutter=\"12\" style=\"margin-top: 20px\">\r\n    <el-col :span=\"24\">\r\n      <el-card shadow=\"always\">\r\n        <el-row :gutter=\"12\">\r\n          <el-col :offset=\"10\" :span=\"12\">\r\n            <div class=\"block\">\r\n              <el-pagination\r\n                :current-page=currentPage\r\n                :page-size=pageSize\r\n                :page-sizes=\"[1, 5, 10, 15, 20, 30]\"\r\n                :total=totalPage\r\n                @current-change=\"handleCurrentChange\"\r\n                @size-change=\"handleSizeChange\"\r\n                background\r\n                layout=\"total, sizes, prev, pager, next, jumper\">\r\n              </el-pagination>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n    </el-col>\r\n  </el-row>\r\n</main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  mounted () {\r\n    this.send()\r\n  },\r\n  name: 'ProductCategory',\r\n  data () {\r\n    return {\r\n      uniqueCodeAO: [],\r\n      data1: [],\r\n      spuCode: this.$route.query.spuCode,\r\n      name: '',\r\n      dialogAddSpecs: false,\r\n      dialogUpdateCategory: false,\r\n      dialogAddCategory: false,\r\n      dialogAddBrand: false,\r\n      formUpdateCategory: {\r\n        categoryCode: '',\r\n        name: '',\r\n        status: true\r\n      },\r\n      formAddCategory: {\r\n        name: '',\r\n        originPrice: '',\r\n        currentPrice: '',\r\n        skuCode: '',\r\n        describe: '',\r\n        status: true\r\n      },\r\n      // 数据表格\r\n      tableData: [],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1,\r\n      allCategoryUrl: 'http://localhost:8443/react/api/getSkuBySpuCode' + '?spuCode='\r\n    }\r\n  },\r\n  methods: {\r\n    handleSelectionChange (val) {\r\n      this.multipleSelection = val\r\n    },\r\n    handleSearchSpu (row) {\r\n      this.$router.push({ path: '/product/ProductSpu', query: { categoryCode: row.categoryCode } })\r\n    },\r\n    checkBrand () {\r\n      let check = this.formAddBrand.code\r\n      this.$axios.post('http://localhost:8443/react/api/checkBrandCode' + '?brandCode=' + check).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$message({\r\n            message: '编号重复',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '编号可使用',\r\n            type: 'success'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    check () {\r\n      let check = this.formAddCategory.categoryCode\r\n      this.$axios.post('http://localhost:8443/react/api/checkExist' + '?categoryCode=' + check).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.$message({\r\n            message: '编号重复',\r\n            type: 'warning'\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '编号可使用',\r\n            type: 'success'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    send () {\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode).then(res => {\r\n        this.tableData = res.data.data.list\r\n        this.totalPage = res.data.data.total\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleAddBrand (row) {\r\n      this.dialogAddBrand = true\r\n    },\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode + '&currentPage=' + this.currentPage + '&pageSize=' + val, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleAddBrandSuccess () {\r\n      let productBrandAO = {\r\n        'categoryCode': this.formAddBrand.categoryCode,\r\n        'code': this.formAddBrand.code,\r\n        'describe': this.formAddBrand.describe,\r\n        'status': this.formAddBrand.status,\r\n        'name': this.formAddBrand.name\r\n      }\r\n      this.$axios.post('http://localhost:8443/react/api/addBrand', productBrandAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('添加品牌成功!')\r\n          this.dialogAddBrand = false\r\n        } else {\r\n          this.$message.error('都跟你讲编号错误了!')\r\n        }\r\n      })\r\n    },\r\n    handleAddBrandClose () {\r\n      this.dialogAddBrand = false\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.$axios.post(this.allCategoryUrl + this.spuCode + '&pageNumber=' + val + '&pageSize=' + this.pageSize, {\r\n      }).then(res => {\r\n        this.tableData = res.data.data.list\r\n        console.log(res)\r\n        console.log(this.tableData)\r\n      })\r\n    },\r\n    handleUpdateSuccess () {\r\n      let productCategoryAO = {\r\n        'categoryCode': this.formUpdateCategory.categoryCode,\r\n        'name': this.formUpdateCategory.name,\r\n        'status': this.formUpdateCategory.status\r\n      }\r\n      this.$axios.post('http://localhost:8443/react/api/updateCategory', productCategoryAO).then(res => {\r\n        if (res.data.code === 1) {\r\n          this.$message.success('修改成功')\r\n          this.send()\r\n          this.dialogUpdateCategory = false\r\n        } else {\r\n          this.$message.error('未知错误')\r\n        }\r\n      })\r\n    },\r\n    handleSearch () {\r\n    },\r\n    handleUpdateClose () {\r\n      this.dialogUpdateCategory = false\r\n    },\r\n    // 修改商品\r\n    handleUpdate (row) {\r\n      this.formUpdateCategory.categoryCode = row.categoryCode\r\n      this.formUpdateCategory.name = row.name\r\n      this.dialogUpdateCategory = true\r\n    },\r\n    // 修改商品状态\r\n    handleUpdateStatus (row) {\r\n      this.$confirm('此操作将修改与分类状态，是否继续?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        row.status = row.status === false\r\n        let productCategoryAO = {\r\n          'uniqueCode': row.uniqueCode,\r\n          'status': row.status\r\n        }\r\n        this.$axios.post('http://localhost:8443/react/api/updateSkuStatue', productCategoryAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('状态修改成功')\r\n          }\r\n        })\r\n      })\r\n    },\r\n    // 添加分类\r\n    handleAdd () {\r\n      this.dialogAddCategory = true\r\n      this.$axios.post('http://localhost:8443/react/api/getSpecs' + '?spuCode=' + this.spuCode).then(res => {\r\n        this.data1 = res.data.data\r\n      })\r\n    },\r\n    handleAddClose () {\r\n      this.$confirm('是否确认取消?', '警告', {\r\n        confirmButtonText: '确认',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message.success('open')\r\n        this.dialogAddCategory = false\r\n      })\r\n        .catch(() => {})\r\n    },\r\n    handleAddSku () {\r\n      this.dialogAddSpecs = true\r\n    },\r\n    handleAddSpecsClose () {\r\n      this.dialogAddSpecs = false\r\n    },\r\n    handleAddSpecsSuccess () {\r\n      for (let i = 0; i < this.data1.length; i++) {\r\n        this.uniqueCodeAO[i] = { value: this.data1[i].name }\r\n      }\r\n      this.dialogAddSpecs = false\r\n    },\r\n    // 添加sku-----------------------------------------------------------------------------------------------\r\n    handleAddSuccess () {\r\n      if (this.uniqueCodeAO.length === 0) {\r\n        this.$message.error('请先为SKU添加一个规格')\r\n      } else {\r\n        let productSkuAO = {\r\n          'spuCode': this.spuCode,\r\n          'skuCode': this.formAddCategory.skuCode,\r\n          'name': this.formAddCategory.name,\r\n          'originPrice': this.formAddCategory.originPrice,\r\n          'currentPrice': this.formAddCategory.currentPrice,\r\n          'describe': this.formAddCategory.describe,\r\n          'status': this.formUpdateCategory.status,\r\n          'uniqueCodeAO': this.uniqueCodeAO\r\n        }\r\n        this.$axios.post('http://localhost:8443/react/api/addProductSku', productSkuAO).then(res => {\r\n          if (res.data.code === 1) {\r\n            this.$message.success('添加商品成功!')\r\n            this.send()\r\n            this.dialogAddCategory = false\r\n          } else {\r\n            this.$message.error('都跟你讲编号错误了!')\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .demo-table-expand {\r\n    font-size: 0;\r\n  }\r\n  .demo-table-expand label {\r\n    width: 90px;\r\n    color: #99a9bf;\r\n  }\r\n  .demo-table-expand .el-form-item {\r\n    margin-right: 0;\r\n    margin-bottom: 0;\r\n    width: 50%;\r\n  }\r\n</style>\r\n"]}]}