n>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"创建人\">\r\n                <template slot-scope=\"scope\">{{ scope.row.creatorName }}</template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"创建时间\">\r\n                <template slot-scope=\"scope\">{{ scope.row.creatorTime }}</template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                align=\"center\"\r\n                label=\"操作\">\r\n                <template slot-scope=\"scope\">\r\n                  <el-button type=\"primary\" plain size=\"small\" @click=\"SpaceValue(scope.row)\">查看</el-button>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n            <div>\r\n              <!--------第二个表格----------->\r\n              <el-drawer\r\n                title= \"属性值\"\r\n                :append-to-body=\"true\"\r\n                :before-close=\"handleClose\"\r\n                :visible.sync=\"innerDrawer\">\r\n                <el-button style=\"margin-left: 375px; margin-bottom: 20px\" type=\"primary\" plain size=\"small\" @click=\"addSpecsValue\">添加值</el-button>\r\n                <el-table :data=\"innerData\"\r\n                          border\r\n                          style=\"width: 100%\"\r\n                          tooltip-effect=\"dark\">\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"属性值\">\r\n                    <template slot-scope=\"scope\">{{ scope.row.name }}</template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    label=\"创建人\">\r\n                    <template slot-scope=\"scope\">{{ scope.row.creatorName }}</template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    