nge=\"handleCurrentChange\"\r\n                  @size-change=\"handleSizeChange\"\r\n                  background\r\n                  layout=\"total, sizes, prev, pager, next, jumper\">\r\n                </el-pagination>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { userLevelList } from '../../api/user'\r\n\r\nexport default {\r\n  created () {\r\n    this.handleLevelList()\r\n  },\r\n  mounted () {\r\n\r\n  },\r\n  data () {\r\n    return {\r\n      // 搜索栏\r\n      searchData: {\r\n        name: '',\r\n        levelId: ''\r\n      },\r\n      name: '',\r\n      levelId: [\r\n        {\r\n          value: 1,\r\n          label: '青铜'\r\n        },\r\n        {\r\n          value: 2,\r\n          label: '白银'\r\n        },\r\n        {\r\n          value: 3,\r\n          label: '黄金'\r\n        },\r\n        {\r\n          value: 4,\r\n          label: '钻石'\r\n        },\r\n        {\r\n          value: 5,\r\n          label: '星耀'\r\n        }\r\n      ],\r\n      // 数据\r\n      tableData: [\r\n        {\r\n          id: '1',\r\n          levelName: '白银',\r\n          levelDiscount: '0.5',\r\n          minConsumption: 100\r\n        }\r\n      ],\r\n      // 每页几条数据\r\n      pageSize: 5,\r\n      // 总页数\r\n      totalPage: 0,\r\n      // 当前页\r\n      currentPage: 1\r\n    }\r\n  },\r\n  methods: {\r\n    // 每页几条数据变化\r\n    handleSizeChange (val) {\r\n      this.pageSize = val\r\n    },\r\n    // 当前页变化\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.handleLevelList()\r\n    },\r\n    // 查询\r\n    handleSearch () {\r\n    },\r\n    // 初始化数据\r\n    handleLevelList () {\r\n      let params = {\r\n        'pageNum': this.currentPage,\r\n        'pageSize': this.pageSize\r\n      }\r\n      userLevelList(params).then(res => {\r\n        this.totalPage = res.data.count\r\n        this.tableData = res.data.cont